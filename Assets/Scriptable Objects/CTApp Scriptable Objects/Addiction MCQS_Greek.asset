    \u03A1\u03C9\u03C4\u03AC\u03C4\u03B5 \u03C4\u03C1\u03B9\u03B3\u03CD\u03C1\u03C9,
    \u03B1\u03BB\u03BB\u03AC \u03BA\u03B1\u03BD\u03B5\u03AF\u03C2 \u03B4\u03B5\u03BD
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03BF \u03B9\u03B4\u03B9\u03BF\u03BA\u03C4\u03AE\u03C4\u03B7\u03C2.
    \u0394\u03AF\u03BD\u03B5\u03C4\u03B5 \u03B1\u03C5\u03C4\u03AC \u03C4\u03B1 \u03C7\u03C1\u03AE\u03BC\u03B1\u03C4\u03B1
    \u03C3\u03C4\u03B7\u03BD \u03B5\u03C0\u03CC\u03BC\u03B5\u03BD\u03B7 \u03C6\u03B9\u03BB\u03B1\u03BD\u03B8\u03C1\u03C9\u03C0\u03B9\u03BA\u03AE
    \u03BF\u03C1\u03B3\u03AC\u03BD\u03C9\u03C3\u03B7 \u03C0\u03BF\u03C5 \u03C3\u03C5\u03BD\u03B1\u03BD\u03C4\u03AC\u03C4\u03B5. "
  - "\u0391\u03BD \u03B2\u03C1\u03B5\u03AF\u03C4\u03B5 \u03AD\u03BD\u03B1\u03BD \u03C1\u03CC\u03BB\u03BF
    \u03C0\u03BF\u03C5 \u03C3\u03B1\u03C2 \u03B5\u03BD\u03B4\u03B9\u03B1\u03C6\u03AD\u03C1\u03B5\u03B9,
    \u03B1\u03BD\u03B1\u03B6\u03B7\u03C4\u03AE\u03C3\u03C4\u03B5 \u03C4\u03BF\u03BD
    \u03C3\u03C4\u03BF \u03B4\u03B9\u03B1\u03B4\u03AF\u03BA\u03C4\u03C5\u03BF \u03BA\u03B1\u03B9
    \u03B4\u03B9\u03B1\u03B2\u03AC\u03C3\u03C4\u03B5 \u03C4\u03B7\u03BD \u03C0\u03B5\u03C1\u03B9\u03B3\u03C1\u03B1\u03C6\u03AE
    \u03C4\u03B7\u03C2 \u03B8\u03AD\u03C3\u03B7\u03C2 \u03B5\u03C1\u03B3\u03B1\u03C3\u03AF\u03B1\u03C2.
    \u0394\u03B5\u03AF\u03C4\u03B5 \u03C0\u03BF\u03B9\u03B1 \u03C0\u03C1\u03BF\u03C3\u03CC\u03BD\u03C4\u03B1,
    \u03B4\u03B5\u03BE\u03B9\u03CC\u03C4\u03B7\u03C4\u03B5\u03C2 \u03BA\u03B1\u03B9
    \u03B5\u03BC\u03C0\u03B5\u03B9\u03C1\u03AF\u03B1 \u03B8\u03B1 \u03C7\u03C1\u03B5\u03B9\u03B1\u03C3\u03C4\u03B5\u03AF\u03C4\u03B5,
    \u03B1\u03C5\u03C4\u03CC \u03B8\u03B1 \u03C3\u03B1\u03C2 \u03B4\u03CE\u03C3\u03B5\u03B9
    \u03AD\u03BD\u03B1 \u03C3\u03B7\u03BC\u03B5\u03AF\u03BF \u03B5\u03BA\u03BA\u03AF\u03BD\u03B7\u03C3\u03B7\u03C2
    \u03BA\u03B1\u03B9 \u03B8\u03B1 \u03B5\u03C0\u03B9\u03C3\u03B7\u03BC\u03AC\u03BD\u03B5\u03B9
    \u03C4\u03BF\u03C5\u03C2 \u03C4\u03BF\u03BC\u03B5\u03AF\u03C2 \u03C3\u03C4\u03BF\u03C5\u03C2
    \u03BF\u03C0\u03BF\u03AF\u03BF\u03C5\u03C2 \u03B8\u03B1 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03B5\u03C1\u03B3\u03B1\u03C3\u03C4\u03B5\u03AF\u03C4\u03B5."
  - "\u0395\u03C0\u03AF\u03BB\u03C5\u03C3\u03B7 \u03C0\u03C1\u03BF\u03B2\u03BB\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD, 
    \u039C\u03B7\u03C7\u03B1\u03BD\u03B9\u03BA\u03AE,  \u0394\u03B7\u03BC\u03B9\u03BF\u03C5\u03C1\u03B3\u03B9\u03BA\u03CC\u03C4\u03B7\u03C4\u03B1."
  - "\u039D\u03B5\u03C1\u03CC,  \u03A6\u03B1\u03C1\u03BC\u03B1\u03BA\u03B5\u03C5\u03C4\u03B9\u03BA\u03AE
    \u03B1\u03B3\u03C9\u03B3\u03AE,  \u03A4\u03C1\u03CC\u03C6\u03B9\u03BC\u03B1, 
    \u03A3\u03C0\u03AF\u03C1\u03C4\u03B1,  \u039C\u03B1\u03C7\u03B1\u03AF\u03C1\u03B9
    \u03C4\u03C3\u03AD\u03C0\u03B7\u03C2,  \u0395\u03C0\u03B9\u03C4\u03C1\u03B1\u03C0\u03AD\u03B6\u03B9\u03B1
    \u03C0\u03B1\u03B9\u03C7\u03BD\u03AF\u03B4\u03B9\u03B1,  \u03A7\u03AC\u03C1\u03C4\u03B7\u03C2"
  - "\u0395\u03C0\u03B9\u03C3\u03C4\u03C1\u03AD\u03C8\u03C4\u03B5 \u03C3\u03C4\u03BF
    \u03C3\u03C0\u03AF\u03C4\u03B9 \u03C3\u03B1\u03C2, \u03BA\u03B1\u03B8\u03CE\u03C2
    \u03B8\u03B1 \u03C7\u03AC\u03C3\u03B5\u03C4\u03B5 \u03BF\u03CD\u03C4\u03C9\u03C2
    \u03AE \u03AC\u03BB\u03BB\u03C9\u03C2 \u03C4\u03B7\u03BD \u03AD\u03BD\u03B1\u03C1\u03BE\u03B7
    \u03C4\u03C9\u03BD \u03B5\u03BE\u03B5\u03C4\u03AC\u03C3\u03B5\u03C9\u03BD. \u0394\u03BF\u03BA\u03B9\u03BC\u03AC\u03C3\u03C4\u03B5
    \u03BE\u03B1\u03BD\u03AC \u03B1\u03CD\u03C1\u03B9\u03BF."
  - "\u039D\u03B1 \u03B1\u03BD\u03B1\u03B3\u03BD\u03C9\u03C1\u03AF\u03C3\u03B5\u03C4\u03B5 
    \u03C4\u03BF \u03B5\u03C0\u03B9\u03BA\u03AF\u03BD\u03B4\u03C5\u03BD\u03BF \u03C0\u03BB\u03B1\u03AF\u03C3\u03B9\u03BF;
    \u03BD\u03B1 \u03B5\u03B3\u03BA\u03B1\u03C4\u03B1\u03BB\u03B5\u03AF\u03C8\u03C4\u03B5
    \u03C4\u03BF\u03BD \u03C4\u03CC\u03C0\u03BF/\u03C4\u03B7\u03BD \u03BA\u03B1\u03C4\u03AC\u03C3\u03C4\u03B1\u03C3\u03B7;
    \u03BD\u03B1 \u03B5\u03C6\u03B1\u03C1\u03BC\u03CC\u03C3\u03B5\u03C4\u03B5 \u03B4\u03B9\u03BA\u03B1\u03B9\u03CE\u03BC\u03B1\u03C4\u03B1
    \u03B4\u03B9\u03B5\u03BA\u03B4\u03AF\u03BA\u03B7\u03C3\u03B7\u03C2; \u039D\u03B1
    \u03BF\u03BD\u03BF\u03BC\u03AC\u03C3\u03B5\u03C4\u03B5 \u03C3\u03C4\u03BF\u03B9\u03C7\u03B5\u03AF\u03B1
    \u03C4\u03B7\u03C2 \u03BA\u03B1\u03C4\u03AC\u03C3\u03C4\u03B1\u03C3\u03B7\u03C2"
  - "\u039D\u03B1 \u03BF\u03BD\u03BF\u03BC\u03AC\u03C3\u03B5\u03C4\u03B5 \u03CC\u03BB\u03B1
    \u03C4\u03B1 \u03C3\u03C4\u03BF\u03B9\u03C7\u03B5\u03AF\u03B1 \u03C4\u03BF\u03C5
    \u03B4\u03C9\u03BC\u03B1\u03C4\u03AF\u03BF\u03C5; \u03BD\u03B1 \u03B5\u03C4\u03BF\u03B9\u03BC\u03AC\u03C3\u03B5\u03C4\u03B5
    \u03BF\u03B4\u03B7\u03B3\u03AF\u03B5\u03C2 \u03B2\u03AE\u03BC\u03B1 \u03C0\u03C1\u03BF\u03C2
    \u03B2\u03AE\u03BC\u03B1; \u03BD\u03B1 \u03BA\u03B1\u03B8\u03BF\u03C1\u03AF\u03C3\u03B5\u03C4\u03B5
    \u03C4\u03BF\u03C5\u03C2 \u03BA\u03B1\u03BD\u03CC\u03BD\u03B5\u03C2 \u03C0\u03BF\u03C5
    \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1 \u03B5\u03C6\u03B1\u03C1\u03BC\u03CC\u03B6\u03BF\u03BD\u03C4\u03B1\u03B9;
    \u03BD\u03B1 \u03B1\u03BD\u03B1\u03B3\u03BD\u03C9\u03C1\u03AF\u03C3\u03B5\u03C4\u03B5
    \u03C0\u03B5\u03C1\u03B9\u03BF\u03C7\u03AD\u03C2 \u03C0\u03BF\u03C5 \u03C3\u03C5\u03BD\u03B4\u03AD\u03BF\u03C5\u03BD
    \u03C3\u03C4\u03BF\u03B9\u03C7\u03B5\u03AF\u03B1"
  - "\u039A\u03B1\u03BD\u03AD\u03BD\u03B1 \u03B1\u03C0\u03CC \u03C4\u03B1 \u03C0\u03B9\u03BF
    \u03C0\u03AC\u03BD\u03C9."
  - "\u0391\u03BD\u03B1\u03B6\u03AE\u03C4\u03B7\u03C3\u03B7 \u03B3\u03B9\u03B1 \u03AD\u03BD\u03B1
    \u03C3\u03C4\u03BF\u03B9\u03C7\u03B5\u03AF\u03BF; \u03C0\u03C1\u03BF\u03C3\u03B8\u03AD\u03C3\u03C4\u03B5
    \u03C4\u03BF \u03C0\u03C1\u03BF\u03CA\u03CC\u03BD \u03C3\u03C4\u03BF \u03BA\u03B1\u03BB\u03AC\u03B8\u03B9
    \u03B1\u03B3\u03BF\u03C1\u03CE\u03BD; \u03B5\u03B9\u03C3\u03AC\u03B3\u03B5\u03C4\u03B5
    \u03C4\u03B7 \u03B4\u03B9\u03B5\u03CD\u03B8\u03C5\u03BD\u03C3\u03B7 \u03C0\u03B1\u03C1\u03AC\u03B4\u03BF\u03C3\u03B7\u03C2;
    \u03C0\u03BB\u03B7\u03C1\u03CE\u03C3\u03B5\u03B9 \u03B3\u03B9\u03B1 \u03C4\u03BF
    \u03B1\u03BD\u03C4\u03B9\u03BA\u03B5\u03AF\u03BC\u03B5\u03BD\u03BF"
  - "\u0391\u03C0\u03B1\u03C1\u03B9\u03B8\u03BC\u03AE\u03C3\u03C4\u03B5 \u03CC\u03BB\u03B1
    \u03C4\u03B1 \u03C5\u03BB\u03B9\u03BA\u03AC, \u03C0\u03B5\u03C1\u03B9\u03B3\u03C1\u03AC\u03C8\u03C4\u03B5
    \u03C0\u03CE\u03C2 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1 \u03C3\u03C5\u03BD\u03B4\u03C5\u03B1\u03C3\u03C4\u03BF\u03CD\u03BD
    \u03B3\u03B9\u03B1 \u03BA\u03AC\u03B8\u03B5 \u03C3\u03C5\u03C3\u03C4\u03B1\u03C4\u03B9\u03BA\u03CC,
    \u03B5\u03BE\u03B7\u03B3\u03AE\u03C3\u03C4\u03B5 \u03C0\u03B9\u03B8\u03B1\u03BD\u03AD\u03C2
    \u03C4\u03C1\u03BF\u03C0\u03BF\u03C0\u03BF\u03B9\u03AE\u03C3\u03B5\u03B9\u03C2
    \u03C3\u03C4\u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE, \u03C0\u03B5\u03C1\u03B9\u03B3\u03C1\u03AC\u03C8\u03C4\u03B5
    \u03CC\u03BB\u03B1 \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03C0\u03BF\u03C5
    \u03C7\u03C1\u03B5\u03B9\u03AC\u03B6\u03BF\u03BD\u03C4\u03B1\u03B9 \u03B3\u03B9\u03B1
    \u03BD\u03B1 \u03C6\u03C4\u03B9\u03AC\u03BE\u03B5\u03C4\u03B5 \u03C4\u03BF \u03BA\u03AD\u03B9\u03BA."
