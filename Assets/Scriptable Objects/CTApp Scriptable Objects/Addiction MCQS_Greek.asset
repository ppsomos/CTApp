    \u03C3\u03B5: - \u0397\u03BB\u03B9\u03BA\u03B9\u03B1\u03BA\u03CC \u03B5\u03CD\u03C1\u03BF\u03C2
    (\u03BA\u03AC\u03C4\u03C9 \u03B1\u03C0\u03CC 30, 30 \u03AD\u03C9\u03C2 60, \u03AC\u03BD\u03C9
    \u03C4\u03C9\u03BD 60 \u03B5\u03C4\u03CE\u03BD) - \u039A\u03B1\u03C4\u03AC\u03C3\u03C4\u03B1\u03C3\u03B7
    \u03C4\u03B1\u03BE\u03B9\u03B4\u03B9\u03BF\u03CD (\u03BC\u03CC\u03BD\u03BF\u03C2,
    \u03B4\u03CD\u03BF \u03AC\u03C4\u03BF\u03BC\u03B1, \u03BF\u03BC\u03AC\u03B4\u03B1)
    - \u03A4\u03B1\u03BE\u03AF\u03B4\u03B9\u03B1 \u03BC\u03B5\u03B3\u03AC\u03BB\u03C9\u03BD
    \u03B1\u03C0\u03BF\u03C3\u03C4\u03AC\u03C3\u03B5\u03C9\u03BD (\u03B5\u03BA\u03C4\u03CC\u03C2
    \u03C4\u03B7\u03C2 \u03C0\u03B5\u03C1\u03B9\u03BF\u03C7\u03AE\u03C2 \u03C3\u03B1\u03C2)
    - \u0391\u03B3\u03BF\u03C1\u03AC\u03C3\u03C4\u03B5 \u03C4\u03B7\u03BD \u03AD\u03BA\u03B8\u03B5\u03C3\u03B7
    \u03B5\u03BA\u03C0\u03C4\u03CE\u03C3\u03B5\u03C9\u03BD \u03B3\u03B9\u03B1 \u03BC\u03B5\u03B3\u03AC\u03BB\u03B7
    \u03C0\u03B5\u03C1\u03AF\u03BF\u03B4\u03BF \u03AE \u03C3\u03CD\u03BD\u03C4\u03BF\u03BC\u03B7
    \u03C0\u03B5\u03C1\u03AF\u03BF\u03B4\u03BF (\u03C0\u03C1\u03BF\u03CE\u03B8\u03B7\u03C3\u03B7
    \u03B5\u03BD\u03CC\u03C2 \u03AD\u03C4\u03BF\u03C5\u03C2 \u03AE 3 \u03B5\u03C4\u03CE\u03BD)
    \u0392\u03C1\u03B5\u03AF\u03C4\u03B5 \u03C4\u03BF\u03BD \u03C0\u03B9\u03BF \u03B1\u03C0\u03BF\u03C4\u03B5\u03BB\u03B5\u03C3\u03BC\u03B1\u03C4\u03B9\u03BA\u03CC
    \u03C4\u03C1\u03CC\u03C0\u03BF \u03B3\u03B9\u03B1 \u03C4\u03BF\u03BD \u03C0\u03C1\u03BF\u03CB\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03BC\u03CC
    \u03C4\u03C9\u03BD \u03C7\u03C1\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD \u03C3\u03B1\u03C2."
  - "\"\u03A8\u03AE\u03BD\u03B5\u03C4\u03B5 \u03C4\u03B1 \u03C0\u03B1\u03C1\u03B1\u03BA\u03AC\u03C4\u03C9
    \u03C0\u03C1\u03BF\u03CA\u03CC\u03BD\u03C4\u03B1:   \u0391 - \u039C\u03C0\u03B9\u03C3\u03BA\u03CC\u03C4\u03B1
    \u03BA\u03B1\u03B9 \u03BA\u03AD\u03B9\u03BA \u0392 - \u039C\u03B1\u03C1\u03AD\u03B3\u03BA\u03B1,
    \u03B3\u03BB\u03AC\u03C3\u03BF, \u03C3\u03B1\u03BD\u03C4\u03B9\u03B3\u03AF \u0393
    - \u03A8\u03C9\u03BC\u03AF \u03BA\u03B1\u03B9 \u03B3\u03BB\u03C5\u03BA\u03AC
    \u03C8\u03C9\u03BC\u03AC\u03BA\u03B9\u03B1\"                                  
    \u03A0\u03BF\u03B9\u03BF \u03BC\u03AF\u03BE\u03B5\u03C1 \u03BA\u03BF\u03C5\u03B6\u03AF\u03BD\u03B1\u03C2
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C0\u03C1\u03BF\u03C4\u03B9\u03BC\u03CC\u03C4\u03B5\u03C1\u03BF
    \u03BD\u03B1 \u03C7\u03C1\u03B7\u03C3\u03B9\u03BC\u03BF\u03C0\u03BF\u03B9\u03AE\u03C3\u03B5\u03C4\u03B5:
    \u03A4\u03B9\u03C0: \u03A4\u03BF \u03BC\u03AF\u03BE\u03B5\u03C1 \u03BA\u03BF\u03C5\u03B6\u03AF\u03BD\u03B1\u03C2
    \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF \u03BD\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03AD\u03BD\u03B1\u03C2 \u03B1\u03BD\u03B1\u03B4\u03B5\u03C5\u03C4\u03AE\u03C1\u03B1\u03C2,
    \u03AD\u03BD\u03B1 \u03C3\u03CD\u03C1\u03BC\u03B1, \u03AD\u03BD\u03B1 \u03B3\u03AC\u03BD\u03C4\u03B6\u03BF
    \u03B6\u03CD\u03BC\u03B7\u03C2, \u03AD\u03BD\u03B1 \u03BA\u03BF\u03C5\u03C4\u03AC\u03BB\u03B9.
    \u03A0\u03BF\u03B9\u03B1 \u03B1\u03C0\u03CC \u03C4\u03B1 \u03C0\u03B1\u03C1\u03B1\u03BA\u03AC\u03C4\u03C9
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C3\u03C9\u03C3\u03C4\u03AC; (\u03C0\u03BF\u03BB\u03BB\u03B1\u03C0\u03BB\u03AD\u03C2
    \u03C3\u03C9\u03C3\u03C4\u03AD\u03C2 \u03B1\u03C0\u03B1\u03BD\u03C4\u03AE\u03C3\u03B5\u03B9\u03C2)"
  - "\u0391\u03BD\u03B1\u03C0\u03C4\u03CD\u03C3\u03C3\u03B5\u03C4\u03B5 \u03AD\u03BD\u03B1
    \u03B5\u03C0\u03B9\u03C4\u03C1\u03B1\u03C0\u03AD\u03B6\u03B9\u03BF \u03C0\u03B1\u03B9\u03C7\u03BD\u03AF\u03B4\u03B9. 
    \u0394\u03B9\u03B1\u03B2\u03AC\u03C3\u03C4\u03B5 \u03C4\u03B9\u03C2 \u03C0\u03B1\u03C1\u03B1\u03BA\u03AC\u03C4\u03C9
    \u03C0\u03C1\u03BF\u03C4\u03AC\u03C3\u03B5\u03B9\u03C2 \u03BA\u03B1\u03B9 \u03B5\u03C0\u03B9\u03BB\u03AD\u03BE\u03C4\u03B5
    \u03C4\u03B7\u03BD \u03C0\u03C1\u03CC\u03C4\u03B1\u03C3\u03B7 \u03C0\u03BF\u03C5
    \u03B4\u03B5\u03BD \u03C3\u03B1\u03C2 \u03B2\u03BF\u03B7\u03B8\u03AC\u03B5\u03B9
    \u03BD\u03B1 \u03B1\u03BD\u03B1\u03C0\u03C4\u03CD\u03BE\u03B5\u03C4\u03B5 \u03AD\u03BD\u03B1
    \u03B5\u03C0\u03B9\u03C4\u03C1\u03B1\u03C0\u03AD\u03B6\u03B9\u03BF \u03C0\u03B1\u03B9\u03C7\u03BD\u03AF\u03B4\u03B9."
  - "\u039C\u03C0\u03B1\u03AF\u03BD\u03B5\u03C4\u03B5 \u03C3\u03B5 \u03AD\u03BD\u03B1
    \u03BA\u03B1\u03C4\u03AC\u03C3\u03C4\u03B7\u03BC\u03B1 \u03BA\u03B1\u03B9 \u03C5\u03C0\u03AC\u03C1\u03C7\u03BF\u03C5\u03BD
    \u03BA\u03AC\u03C0\u03BF\u03B9\u03B1 \u03BC\u03B5\u03C4\u03C1\u03B7\u03C4\u03AC
    \u03C3\u03C4\u03BF \u03C0\u03AC\u03C4\u03C9\u03BC\u03B1. \u03A4\u03B9 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03BA\u03AC\u03BD\u03B5\u03C4\u03B5 \u03B1\u03C0\u03CC \u03B7\u03B8\u03B9\u03BA\u03AE\u03C2
    \u03AC\u03C0\u03BF\u03C8\u03B7\u03C2; \u0395\u03C0\u03B9\u03BB\u03AD\u03BE\u03C4\u03B5
    \u03C4\u03B7\u03BD \u03BA\u03B1\u03C4\u03B1\u03BB\u03BB\u03B7\u03BB\u03CC\u03C4\u03B5\u03C1\u03B7
    \u03B7\u03B8\u03B9\u03BA\u03AE \u03B1\u03C0\u03AC\u03BD\u03C4\u03B7\u03C3\u03B7
    \u03C3\u03B5 \u03C3\u03C5\u03BD\u03AC\u03C1\u03C4\u03B7\u03C3\u03B7 \u03BC\u03B5
    \u03C4\u03B1 \u03C3\u03B5\u03BD\u03AC\u03C1\u03B9\u03B1."
  - "\u0398\u03AD\u03BB\u03B5\u03C4\u03B5 \u03BD\u03B1 \u03C0\u03C1\u03BF\u03C3\u03BB\u03B7\u03C6\u03B8\u03B5\u03AF\u03C4\u03B5
    \u03B3\u03B9\u03B1 \u03BC\u03B9\u03B1 \u03C3\u03C5\u03B3\u03BA\u03B5\u03BA\u03C1\u03B9\u03BC\u03AD\u03BD\u03B7
    \u03B5\u03C1\u03B3\u03B1\u03C3\u03AF\u03B1.  \u0394\u03B9\u03B1\u03B2\u03AC\u03C3\u03C4\u03B5
    \u03C4\u03B9\u03C2 \u03C0\u03B1\u03C1\u03B1\u03BA\u03AC\u03C4\u03C9 \u03C0\u03C1\u03BF\u03C4\u03AC\u03C3\u03B5\u03B9\u03C2
    \u03BA\u03B1\u03B9 \u03B5\u03C0\u03B9\u03BB\u03AD\u03BE\u03C4\u03B5 \u03C4\u03B7\u03BD
    \u03B1\u03C0\u03AC\u03BD\u03C4\u03B7\u03C3\u03B7 \u03C0\u03BF\u03C5 \u03B4\u03B5\u03BD
    \u03B8\u03B1 \u03C3\u03B1\u03C2 \u03B2\u03BF\u03B7\u03B8\u03BF\u03CD\u03C3\u03B5."
  - "\u0394\u03B9\u03B1\u03B2\u03AC\u03C3\u03C4\u03B5 \u03C4\u03B9\u03C2 \u03C0\u03B1\u03C1\u03B1\u03BA\u03AC\u03C4\u03C9
    \u03C0\u03C1\u03BF\u03C4\u03AC\u03C3\u03B5\u03B9\u03C2 \u03BA\u03B1\u03B9 \u03B5\u03C0\u03B9\u03BB\u03AD\u03BE\u03C4\u03B5
    \u03C4\u03B7\u03BD \u03B1\u03C0\u03AC\u03BD\u03C4\u03B7\u03C3\u03B7 \u03C0\u03BF\u03C5
    \u03C0\u03B5\u03C1\u03B9\u03AD\u03C7\u03B5\u03B9 \u039C\u039F\u039D\u039F \u03C4\u03B9\u03C2
    \u03BA\u03BF\u03B9\u03BD\u03C9\u03BD\u03B9\u03BA\u03AD\u03C2 \u03B4\u03B5\u03BE\u03B9\u03CC\u03C4\u03B7\u03C4\u03B5\u03C2.
    \u039F\u03B9 \u03BA\u03BF\u03B9\u03BD\u03C9\u03BD\u03B9\u03BA\u03AD\u03C2 \u03B4\u03B5\u03BE\u03B9\u03CC\u03C4\u03B7\u03C4\u03B5\u03C2
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C7\u03B1\u03C1\u03B1\u03BA\u03C4\u03B7\u03C1\u03B9\u03C3\u03C4\u03B9\u03BA\u03AC
    \u03BA\u03B1\u03B9 \u03B3\u03BD\u03C9\u03C1\u03AF\u03C3\u03BC\u03B1\u03C4\u03B1
    \u03C0\u03BF\u03C5 \u03B4\u03B9\u03B1\u03B8\u03AD\u03C4\u03B5\u03C4\u03B5 \u03C9\u03C2
    \u03AC\u03C4\u03BF\u03BC\u03BF, \u03C4\u03B1 \u03BF\u03C0\u03BF\u03AF\u03B1 \u03B1\u03BD\u03B1\u03C0\u03C4\u03CD\u03C3\u03C3\u03BF\u03BD\u03C4\u03B1\u03B9
    \u03BC\u03B5 \u03C4\u03B7\u03BD \u03C0\u03AC\u03C1\u03BF\u03B4\u03BF \u03C4\u03BF\u03C5
    \u03C7\u03C1\u03CC\u03BD\u03BF\u03C5 \u03BC\u03AD\u03C3\u03C9 \u03C4\u03B7\u03C2
    \u03B3\u03B5\u03BD\u03B9\u03BA\u03AE\u03C2 \u03B5\u03BC\u03C0\u03B5\u03B9\u03C1\u03AF\u03B1\u03C2
    \u03C4\u03B7\u03C2 \u03B6\u03C9\u03AE\u03C2. \u039F\u03B9 \u03C3\u03BA\u03BB\u03B7\u03C1\u03AD\u03C2
    \u03B4\u03B5\u03BE\u03B9\u03CC\u03C4\u03B7\u03C4\u03B5\u03C2 \u03B1\u03C0\u03CC
    \u03C4\u03B7\u03BD \u03AC\u03BB\u03BB\u03B7 \u03C0\u03BB\u03B5\u03C5\u03C1\u03AC
    \u03B5\u03C0\u03B9\u03BA\u03B5\u03BD\u03C4\u03C1\u03CE\u03BD\u03BF\u03BD\u03C4\u03B1\u03B9
    \u03C3\u03C4\u03B7\u03BD \u03B9\u03BA\u03B1\u03BD\u03CC\u03C4\u03B7\u03C4\u03AC
    \u03C3\u03B1\u03C2 \u03BD\u03B1 \u03BA\u03AC\u03BD\u03B5\u03C4\u03B5 \u03C3\u03C5\u03B3\u03BA\u03B5\u03BA\u03C1\u03B9\u03BC\u03AD\u03BD\u03B5\u03C2
    \u03B5\u03C1\u03B3\u03B1\u03C3\u03AF\u03B5\u03C2."
  - "\u03A6\u03B1\u03BD\u03C4\u03B1\u03C3\u03C4\u03B5\u03AF\u03C4\u03B5 \u03AD\u03BD\u03B1
    \u03C3\u03B5\u03BD\u03AC\u03C1\u03B9\u03BF \u03CC\u03C0\u03BF\u03C5 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03B1\u03C0\u03BF\u03B4\u03B5\u03AF\u03BE\u03B5\u03C4\u03B5 \u03C4\u03B9\u03C2
    \u03B9\u03BA\u03B1\u03BD\u03CC\u03C4\u03B7\u03C4\u03AD\u03C2 \u03C3\u03B1\u03C2
    \u03C3\u03C4\u03B7\u03BD \u03B5\u03C0\u03B9\u03B2\u03AF\u03C9\u03C3\u03B7 \u03BA\u03B1\u03B9
    \u03BD\u03B1 \u03C0\u03B1\u03BA\u03B5\u03C4\u03AC\u03C1\u03B5\u03C4\u03B5 \u03C4\u03B7\u03BD
    \u03C4\u03C3\u03AC\u03BD\u03C4\u03B1 \u03BC\u03B5 \u03B1\u03BD\u03C4\u03B9\u03BA\u03B5\u03AF\u03BC\u03B5\u03BD\u03B1
    \u03C0\u03BF\u03C5 \u03B8\u03B1 \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03C3\u03B1\u03BD
    \u03BD\u03B1 \u03C3\u03B1\u03C2 \u03B2\u03BF\u03B7\u03B8\u03AE\u03C3\u03BF\u03C5\u03BD. 
    \u0394\u03B9\u03B1\u03B2\u03AC\u03C3\u03C4\u03B5 \u03C4\u03B9\u03C2 \u03C0\u03B1\u03C1\u03B1\u03BA\u03AC\u03C4\u03C9
    \u03C0\u03C1\u03BF\u03C4\u03AC\u03C3\u03B5\u03B9\u03C2 \u03BA\u03B1\u03B9 \u03B5\u03C0\u03B9\u03BB\u03AD\u03BE\u03C4\u03B5
    \u03C4\u03B7\u03BD \u03B1\u03C0\u03AC\u03BD\u03C4\u03B7\u03C3\u03B7 \u03C0\u03BF\u03C5
    \u03B4\u03B5\u03BD \u03C0\u03B5\u03C1\u03B9\u03AD\u03C7\u03B5\u03B9 \u03BC\u03CC\u03BD\u03BF
    \u03B2\u03B1\u03C3\u03B9\u03BA\u03AC \u03C3\u03CD\u03B3\u03C7\u03C1\u03BF\u03BD\u03B1
    \u03B1\u03BD\u03C4\u03B9\u03BA\u03B5\u03AF\u03BC\u03B5\u03BD\u03B1 \u03B5\u03C0\u03B9\u03B2\u03AF\u03C9\u03C3\u03B7\u03C2."
  - "\u03A4\u03BF \u03C4\u03C1\u03AD\u03BD\u03BF \u03C3\u03B1\u03C2 \u03B3\u03B9\u03B1
    \u03C4\u03BF \u03C3\u03C7\u03BF\u03BB\u03B5\u03AF\u03BF \u03B1\u03BA\u03C5\u03C1\u03CE\u03B8\u03B7\u03BA\u03B5.
    \u0394\u03B9\u03B1\u03B2\u03AC\u03C3\u03C4\u03B5 \u03C4\u03B9\u03C2 \u03C0\u03B1\u03C1\u03B1\u03BA\u03AC\u03C4\u03C9
    \u03C0\u03C1\u03BF\u03C4\u03AC\u03C3\u03B5\u03B9\u03C2 \u03BA\u03B1\u03B9 \u03B5\u03C0\u03B9\u03BB\u03AD\u03BE\u03C4\u03B5
    \u03C4\u03B7\u03BD \u03B1\u03C0\u03AC\u03BD\u03C4\u03B7\u03C3\u03B7 \u03C0\u03BF\u03C5
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B7 \u03BB\u03B9\u03B3\u03CC\u03C4\u03B5\u03C1\u03BF
    \u03BA\u03BF\u03C3\u03C4\u03BF\u03B2\u03CC\u03C1\u03B1 \u03BA\u03B1\u03B9 \u03BB\u03B9\u03B3\u03CC\u03C4\u03B5\u03C1\u03BF
    \u03C7\u03C1\u03BF\u03BD\u03BF\u03B2\u03CC\u03C1\u03B1 \u03B5\u03C0\u03B9\u03BB\u03BF\u03B3\u03AE."
  - "\u0393\u03B9\u03B1 \u03BD\u03B1 \u03B1\u03BD\u03C4\u03B9\u03C3\u03C4\u03B1\u03B8\u03B5\u03AF\u03C4\u03B5
    \u03C3\u03C4\u03B7\u03BD \u03B1\u03BD\u03B5\u03C0\u03B9\u03B8\u03CD\u03BC\u03B7\u03C4\u03B7
    \u03C0\u03AF\u03B5\u03C3\u03B7 \u03BD\u03B1 \u03BA\u03AC\u03BD\u03B5\u03C4\u03B5
    \u03C4\u03BF \u03BB\u03AC\u03B8\u03BF\u03C2, \u03B8\u03B1 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AE\u03C3\u03B5\u03C4\u03B5
    \u03B1\u03C5\u03C4\u03AC \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03BC\u03B5
    \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC \u03C0\u03BF\u03C5 \u03B4\u03AF\u03BD\u03B5\u03C4\u03B1\u03B9:"
  - "\u0393\u03B9\u03B1 \u03BD\u03B1 \u03B4\u03B9\u03B4\u03AC\u03BE\u03B5\u03C4\u03B5
    \u03C3\u03C4\u03BF \u03C1\u03BF\u03BC\u03C0\u03CC\u03C4 \u03C0\u03CE\u03C2 \u03BD\u03B1
    \u03BA\u03B1\u03B8\u03B1\u03C1\u03AF\u03B6\u03B5\u03B9 \u03AD\u03BD\u03B1 \u03B4\u03C9\u03BC\u03AC\u03C4\u03B9\u03BF,
    \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1 \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AE\u03C3\u03B5\u03C4\u03B5
    \u03C4\u03B1 \u03C0\u03B1\u03C1\u03B1\u03BA\u03AC\u03C4\u03C9 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1
    \u03BC\u03B5 \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC \u03C0\u03BF\u03C5 \u03B4\u03AF\u03BD\u03B5\u03C4\u03B1\u03B9:"
  - "\u0393\u03B9\u03B1 \u03BD\u03B1 \u03B4\u03B9\u03B4\u03AC\u03BE\u03B5\u03C4\u03B5
    \u03C3\u03C4\u03BF \u03C1\u03BF\u03BC\u03C0\u03CC\u03C4 \u03C0\u03CE\u03C2 \u03BD\u03B1
    \u03BA\u03B1\u03B8\u03B1\u03C1\u03AF\u03B6\u03B5\u03B9 \u03AD\u03BD\u03B1 \u03B4\u03C9\u03BC\u03AC\u03C4\u03B9\u03BF,
    \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1 \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AE\u03C3\u03B5\u03C4\u03B5
    \u03C4\u03B1 \u03C0\u03B1\u03C1\u03B1\u03BA\u03AC\u03C4\u03C9 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1
    \u03BC\u03B5 \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC \u03C0\u03BF\u03C5 \u03B4\u03AF\u03BD\u03B5\u03C4\u03B1\u03B9:"
  - "\u038C\u03C4\u03B1\u03BD \u03B3\u03C1\u03AC\u03C6\u03B5\u03C4\u03B5 \u03AD\u03BD\u03B1
    \u03C0\u03C1\u03CC\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1 \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03C4\u03C5\u03B1\u03BA\u03CE\u03BD
    \u03B1\u03B3\u03BF\u03C1\u03CE\u03BD, \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1
    \u03B5\u03C0\u03B9\u03BB\u03AD\u03BE\u03B5\u03C4\u03B5 \u03C4\u03B7 \u03C3\u03C9\u03C3\u03C4\u03AE
    \u03C3\u03B5\u03B9\u03C1\u03AC \u03C4\u03C9\u03BD \u03B5\u03C1\u03B3\u03B1\u03C3\u03B9\u03CE\u03BD
    \u03C0\u03BF\u03C5 \u03B8\u03B1 \u03B5\u03BA\u03C4\u03B5\u03BB\u03AD\u03C3\u03B5\u03B9:"
  - "\u0393\u03B9\u03B1 \u03BD\u03B1 \u03B5\u03C4\u03BF\u03B9\u03BC\u03AC\u03C3\u03B5\u03C4\u03B5
    \u03BC\u03B9\u03B1 \u03BA\u03B1\u03BB\u03AE \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE
    \u03B3\u03B9\u03B1 \u03BA\u03AD\u03B9\u03BA, \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AE\u03C3\u03C4\u03B5
    \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03BC\u03B5 \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC
    \u03C0\u03BF\u03C5 \u03B4\u03AF\u03BD\u03BF\u03BD\u03C4\u03B1\u03B9:"
  Question_Explanation:
  - "\u0397 \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE
    \u03C3\u03BA\u03AD\u03C8\u03B7 \u03AD\u03C7\u03B5\u03B9 \u03BD\u03B1 \u03BA\u03AC\u03BD\u03B5\u03B9
    \u03BC\u03B5 \u03C4\u03B7 \u03B4\u03B9\u03AC\u03C3\u03C0\u03B1\u03C3\u03B7 \u03C4\u03C9\u03BD
    \u03B1\u03C0\u03BF\u03C6\u03AC\u03C3\u03B5\u03C9\u03BD \u03C3\u03B5 \u03BC\u03B9\u03BA\u03C1\u03CC\u03C4\u03B5\u03C1\u03B1
    \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1, \u03CE\u03C3\u03C4\u03B5 \u03BD\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03C0\u03B9\u03BF \u03B5\u03CD\u03BA\u03BF\u03BB\u03B1 \u03B4\u03B9\u03B1\u03C7\u03B5\u03B9\u03C1\u03AF\u03C3\u03B9\u03BC\u03B1.
    \u0391\u03C5\u03C4\u03CC \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF \u03B5\u03C0\u03AF\u03C3\u03B7\u03C2
    \u03BD\u03B1 \u03B5\u03C6\u03B1\u03C1\u03BC\u03BF\u03C3\u03C4\u03B5\u03AF \u03B3\u03B9\u03B1
    \u03BD\u03B1 \u03B1\u03C0\u03BF\u03C6\u03B1\u03C3\u03AF\u03C3\u03B5\u03C4\u03B5
    \u03C0\u03CE\u03C2 \u03BA\u03B1\u03B9 \u03C0\u03CC\u03C4\u03B5 \u03B8\u03B1 \u03C4\u03B1\u03BE\u03B9\u03B4\u03AD\u03C8\u03B5\u03C4\u03B5.
    \u03A0\u03C1\u03CE\u03C4\u03BF\u03BD, \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1
    \u03C0\u03C1\u03BF\u03C3\u03B4\u03B9\u03BF\u03C1\u03AF\u03C3\u03B5\u03C4\u03B5
    \u03C0\u03CC\u03C4\u03B5 \u03B8\u03AD\u03BB\u03B5\u03C4\u03B5 \u03BD\u03B1 \u03C4\u03B1\u03BE\u03B9\u03B4\u03AD\u03C8\u03B5\u03C4\u03B5,
    \u03BC\u03B5 \u03B2\u03AC\u03C3\u03B7 \u03C4\u03B7 \u03B4\u03B9\u03B1\u03B8\u03B5\u03C3\u03B9\u03BC\u03CC\u03C4\u03B7\u03C4\u03AC
    \u03C3\u03B1\u03C2. \u03A3\u03C4\u03B7 \u03C3\u03C5\u03BD\u03AD\u03C7\u03B5\u03B9\u03B1,
    \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1 \u03B1\u03BD\u03B1\u03B6\u03B7\u03C4\u03AE\u03C3\u03B5\u03C4\u03B5
    \u03CC\u03BB\u03B5\u03C2 \u03C4\u03B9\u03C2 \u03B4\u03B9\u03B1\u03C6\u03BF\u03C1\u03B5\u03C4\u03B9\u03BA\u03AD\u03C2
    \u03B5\u03C0\u03B9\u03BB\u03BF\u03B3\u03AD\u03C2 \u03C0\u03BF\u03C5 \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF
    \u03BD\u03B1 \u03AD\u03C7\u03B5\u03C4\u03B5. \u0393\u03B9\u03B1 \u03C0\u03B1\u03C1\u03AC\u03B4\u03B5\u03B9\u03B3\u03BC\u03B1,
    \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF \u03BD\u03B1 \u03B8\u03AD\u03BB\u03B5\u03C4\u03B5
    \u03BD\u03B1 \u03C3\u03BA\u03B5\u03C6\u03C4\u03B5\u03AF\u03C4\u03B5 \u03BD\u03B1
    \u03C4\u03B1\u03BE\u03B9\u03B4\u03AD\u03C8\u03B5\u03C4\u03B5 \u03BC\u03B5 \u03B1\u03C5\u03C4\u03BF\u03BA\u03AF\u03BD\u03B7\u03C4\u03BF,
    \u03C4\u03C1\u03AD\u03BD\u03BF \u03AE \u03B1\u03BA\u03CC\u03BC\u03B7 \u03BA\u03B1\u03B9
    \u03BC\u03B5 \u03B1\u03B5\u03C1\u03BF\u03C0\u03BB\u03AC\u03BD\u03BF. \u039C\u03C0\u03BF\u03C1\u03B5\u03AF\u03C4\u03B5
    \u03B5\u03C0\u03AF\u03C3\u03B7\u03C2 \u03BD\u03B1 \u03B1\u03BD\u03B1\u03B6\u03B7\u03C4\u03AE\u03C3\u03B5\u03C4\u03B5
    \u03C0\u03B9\u03BF \u03C0\u03C1\u03BF\u03C3\u03B9\u03C4\u03AD\u03C2 \u03B5\u03C0\u03B9\u03BB\u03BF\u03B3\u03AD\u03C2,
    \u03B3\u03B9\u03B1 \u03C0\u03B1\u03C1\u03AC\u03B4\u03B5\u03B9\u03B3\u03BC\u03B1
    \u03C0\u03C1\u03BF\u03C3\u03B8\u03AD\u03C4\u03BF\u03BD\u03C4\u03B1\u03C2 \u03BC\u03B9\u03B1
    \u03C0\u03C4\u03AE\u03C3\u03B7 \u03BC\u03B5 \u03B5\u03BD\u03B4\u03B9\u03AC\u03BC\u03B5\u03C3\u03BF
    \u03C3\u03C4\u03B1\u03B8\u03BC\u03CC. \u0395\u03C6\u03CC\u03C3\u03BF\u03BD \u03AD\u03C7\u03B5\u03C4\u03B5
    \u03C0\u03BB\u03AD\u03BF\u03BD \u03CC\u03BB\u03B5\u03C2 \u03C4\u03B9\u03C2 \u03C0\u03BB\u03B7\u03C1\u03BF\u03C6\u03BF\u03C1\u03AF\u03B5\u03C2
    \u03C0\u03BF\u03C5 \u03C7\u03C1\u03B5\u03B9\u03AC\u03B6\u03B5\u03C3\u03C4\u03B5
    \u03BA\u03B1\u03B9 \u03AD\u03C7\u03B5\u03C4\u03B5 \u03B4\u03B7\u03BC\u03B9\u03BF\u03C5\u03C1\u03B3\u03AE\u03C3\u03B5\u03B9
    \u03C0\u03BF\u03BB\u03BB\u03B1\u03C0\u03BB\u03AC \u03C3\u03B5\u03BD\u03AC\u03C1\u03B9\u03B1,
    \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF\u03C4\u03B5 \u03BD\u03B1 \u03B5\u03C0\u03B9\u03BB\u03AD\u03BE\u03B5\u03C4\u03B5
    \u03B1\u03C5\u03C4\u03CC \u03C0\u03BF\u03C5 \u03C3\u03B1\u03C2 \u03C4\u03B1\u03B9\u03C1\u03B9\u03AC\u03B6\u03B5\u03B9
    \u03BA\u03B1\u03BB\u03CD\u03C4\u03B5\u03C1\u03B1!"
  - "\u039C\u03B5\u03C4\u03B1\u03BE\u03CD \u03CC\u03BB\u03C9\u03BD \u03C4\u03C9\u03BD
    \u03C0\u03BB\u03B7\u03C1\u03BF\u03C6\u03BF\u03C1\u03B9\u03CE\u03BD \u03C0\u03BF\u03C5
    \u03B8\u03B1 \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03C3\u03B5 \u03BD\u03B1 \u03C0\u03B5\u03C1\u03B9\u03AD\u03C7\u03B5\u03B9
    \u03B7 \u03B4\u03B9\u03B1\u03C6\u03AE\u03BC\u03B9\u03C3\u03B7, \u03B7 \u03B7\u03BC\u03B5\u03C1\u03BF\u03BC\u03B7\u03BD\u03AF\u03B1
    \u03C4\u03B7\u03C2 \u03C0\u03B5\u03C1\u03B9\u03BF\u03B4\u03B5\u03AF\u03B1\u03C2
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B7 \u03C0\u03B9\u03BF \u03C3\u03C7\u03B5\u03C4\u03B9\u03BA\u03AE!"
  - "\u038C\u03C4\u03B1\u03BD \u03B1\u03C0\u03BF\u03C6\u03B1\u03C3\u03AF\u03B6\u03B5\u03C4\u03B5
    \u03BD\u03B1 \u03BA\u03AC\u03BD\u03B5\u03C4\u03B5 \u03BA\u03AC\u03C4\u03B9 \u03AE
    \u03BD\u03B1 \u03BB\u03CD\u03C3\u03B5\u03C4\u03B5 \u03AD\u03BD\u03B1 \u03C0\u03C1\u03CC\u03B2\u03BB\u03B7\u03BC\u03B1,
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C0\u03BF\u03BB\u03CD \u03C3\u03B7\u03BC\u03B1\u03BD\u03C4\u03B9\u03BA\u03CC
    \u03BD\u03B1 \u03C3\u03BA\u03B5\u03C6\u03C4\u03B5\u03AF\u03C4\u03B5 \u03C4\u03B1
    \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1! \u03A3\u03B5 \u03B1\u03C5\u03C4\u03CC \u03C4\u03BF
    \u03C0\u03B1\u03C1\u03AC\u03B4\u03B5\u03B9\u03B3\u03BC\u03B1, \u03C4\u03BF \u03C0\u03C1\u03CE\u03C4\u03BF
    \u03C0\u03C1\u03AC\u03B3\u03BC\u03B1 \u03C0\u03BF\u03C5 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03BA\u03AC\u03BD\u03B5\u03C4\u03B5 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03BD\u03B1 \u03B5\u03C1\u03B5\u03C5\u03BD\u03AE\u03C3\u03B5\u03C4\u03B5 \u03C4\u03B9\u03C2
    \u03B5\u03C0\u03B9\u03BB\u03BF\u03B3\u03AD\u03C2 \u03B5\u03BA\u03C0\u03B1\u03AF\u03B4\u03B5\u03C5\u03C3\u03B7\u03C2
    \u03C0\u03B9\u03BB\u03CC\u03C4\u03BF\u03C5, \u03C3\u03C4\u03B7 \u03C3\u03C5\u03BD\u03AD\u03C7\u03B5\u03B9\u03B1
    \u03BD\u03B1 \u03C0\u03B5\u03C1\u03AC\u03C3\u03B5\u03C4\u03B5 \u03CC\u03BB\u03B5\u03C2
    \u03C4\u03B9\u03C2 \u03B1\u03C0\u03B1\u03C1\u03B1\u03AF\u03C4\u03B7\u03C4\u03B5\u03C2
    \u03B9\u03B1\u03C4\u03C1\u03B9\u03BA\u03AD\u03C2 \u03B5\u03BE\u03B5\u03C4\u03AC\u03C3\u03B5\u03B9\u03C2
    \u03C0\u03BF\u03C5 \u03B8\u03B1 \u03C3\u03B1\u03C2 \u03C0\u03C1\u03BF\u03C4\u03B5\u03AF\u03BD\u03BF\u03C5\u03BD,
    \u03BD\u03B1 \u03C0\u03B1\u03C1\u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AE\u03C3\u03B5\u03C4\u03B5
    \u03C4\u03B1 \u03BC\u03B1\u03B8\u03AE\u03BC\u03B1\u03C4\u03B1 \u03C0\u03C4\u03B7\u03C4\u03B9\u03BA\u03AE\u03C2
    \u03B5\u03BA\u03C0\u03B1\u03AF\u03B4\u03B5\u03C5\u03C3\u03B7\u03C2 \u03BA\u03B1\u03B9,
    \u03C4\u03AD\u03BB\u03BF\u03C2, \u03BD\u03B1 \u03B4\u03CE\u03C3\u03B5\u03C4\u03B5
    \u03C4\u03B9\u03C2 \u03B5\u03BE\u03B5\u03C4\u03AC\u03C3\u03B5\u03B9\u03C2 \u03C3\u03B1\u03C2
    \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03C0\u03AC\u03C1\u03B5\u03C4\u03B5 \u03C4\u03B7\u03BD
    \u03C0\u03B9\u03C3\u03C4\u03BF\u03C0\u03BF\u03AF\u03B7\u03C3\u03AE \u03C3\u03B1\u03C2."
  - "\u0397 \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE
    \u03C3\u03BA\u03AD\u03C8\u03B7 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C0\u03C1\u03B1\u03B3\u03BC\u03B1\u03C4\u03B9\u03BA\u03AC
    \u03C0\u03BF\u03BB\u03CD\u03C4\u03B9\u03BC\u03B7 \u03BA\u03B1\u03B9 \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF
    \u03BD\u03B1 \u03BC\u03B1\u03C2 \u03B4\u03B9\u03B5\u03C5\u03BA\u03BF\u03BB\u03CD\u03BD\u03B5\u03B9
    \u03C3\u03C4\u03B7\u03BD \u03B5\u03C0\u03AF\u03BB\u03C5\u03C3\u03B7 \u03C0\u03C1\u03BF\u03B2\u03BB\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD,
    \u03C3\u03C4\u03B7\u03BD \u03B5\u03C0\u03B9\u03BA\u03BF\u03B9\u03BD\u03C9\u03BD\u03AF\u03B1
    \u03BA\u03B1\u03B9 \u03C4\u03B7 \u03C3\u03C5\u03BD\u03B5\u03C1\u03B3\u03B1\u03C3\u03AF\u03B1
    \u03BC\u03B5 \u03AC\u03BB\u03BB\u03BF\u03C5\u03C2 \u03BA\u03B1\u03B9 \u03C3\u03C4\u03B7
    \u03B4\u03B9\u03B1\u03C7\u03B5\u03AF\u03C1\u03B9\u03C3\u03B7 \u03C4\u03B7\u03C2
    \u03B6\u03C9\u03AE\u03C2 \u03BA\u03B1\u03BB\u03CD\u03C4\u03B5\u03C1\u03B1!"
  - "\u0391\u03C2 \u03B1\u03C0\u03BB\u03BF\u03C0\u03BF\u03B9\u03AE\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03B1\u03C5\u03C4\u03CC \u03C4\u03BF \u03C0\u03C1\u03CC\u03B2\u03BB\u03B7\u03BC\u03B1!
    \u0397 \u03C0\u03C1\u03CE\u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC \u03B1\u03C0\u03BF\u03C4\u03B5\u03BB\u03B5\u03AF\u03C4\u03B1\u03B9
    \u03B1\u03C0\u03CC 5 \u03C4\u03BF\u03CD\u03B2\u03BB\u03B1. \u0397 \u03B4\u03B5\u03CD\u03C4\u03B5\u03C1\u03B7
    \u03B1\u03C0\u03BF\u03C4\u03B5\u03BB\u03B5\u03AF\u03C4\u03B1\u03B9 \u03B1\u03C0\u03CC
    5+4=9 \u03C4\u03BF\u03CD\u03B2\u03BB\u03B1, \u03BA\u03B1\u03B9 \u03BF\u03CD\u03C4\u03C9
    \u03BA\u03B1\u03B8\u03B5\u03BE\u03AE\u03C2. \u0393\u03B9\u03B1 \u03BD\u03B1 \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03AF\u03C3\u03B5\u03C4\u03B5
    \u03C4\u03B7 15\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC, \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF\u03C4\u03B5
    \u03C0\u03C1\u03CE\u03C4\u03B1 \u03BD\u03B1 \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03AF\u03C3\u03B5\u03C4\u03B5
    \u03C4\u03B9\u03C2 14 \u03C3\u03B5\u03B9\u03C1\u03AD\u03C2 \u03B5\u03BA\u03C4\u03CC\u03C2
    \u03B1\u03C0\u03CC \u03C4\u03B7\u03BD \u03C0\u03C1\u03CE\u03C4\u03B7, \u03C0\u03BF\u03BB\u03BB\u03B1\u03C0\u03BB\u03B1\u03C3\u03B9\u03AC\u03B6\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03B1\u03C0\u03BB\u03CE\u03C2 14 \u03C3\u03B5\u03B9\u03C1\u03AD\u03C2 x 4 \u03BD\u03AD\u03B1
    \u03C4\u03BF\u03CD\u03B2\u03BB\u03B1. \u03A0\u03C1\u03BF\u03C3\u03B8\u03AD\u03C4\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03BA\u03B1\u03B9 \u03C4\u03B7\u03BD \u03C0\u03C1\u03CE\u03C4\u03B7, \u03B7
    15\u03B7 \u03B3\u03C1\u03B1\u03BC\u03BC\u03AE \u03B1\u03C0\u03BF\u03C4\u03B5\u03BB\u03B5\u03AF\u03C4\u03B1\u03B9
    \u03B1\u03C0\u03CC 5 + (4x14) = 61 \u03C4\u03BF\u03CD\u03B2\u03BB\u03B1."
  - "\u0391\u03C2 \u03C4\u03BF \u03C3\u03BA\u03B5\u03C6\u03C4\u03BF\u03CD\u03BC\u03B5
    \u03BA\u03B1\u03BB\u03AC! \u039B\u03BF\u03B9\u03C0\u03CC\u03BD, \u03B7 \u03BF\u03BC\u03AC\u03B4\u03B1
    \u0391 \u03AD\u03C7\u03B5\u03B9 35 \u03BC\u03B1\u03B8\u03B7\u03C4\u03AD\u03C2,
    \u03C0\u03BF\u03C5 \u03C3\u03B7\u03BC\u03B1\u03AF\u03BD\u03B5\u03B9 \u03CC\u03C4\u03B9
    90 - 35 = 55 \u03BC\u03B1\u03B8\u03B7\u03C4\u03AD\u03C2 \u03C0\u03BF\u03C5 \u03B1\u03BD\u03AE\u03BA\u03BF\u03C5\u03BD
    \u03B5\u03AF\u03C4\u03B5 \u03C3\u03C4\u03B7\u03BD \u03BF\u03BC\u03AC\u03B4\u03B1
    \u0392 \u03B5\u03AF\u03C4\u03B5 \u03C3\u03C4\u03B7\u03BD \u03BF\u03BC\u03AC\u03B4\u03B1
    \u0393. \u0397 \u03BF\u03BC\u03AC\u03B4\u03B1 \u0392 \u03AD\u03C7\u03B5\u03B9
    15 \u03C0\u03B5\u03C1\u03B9\u03C3\u03C3\u03CC\u03C4\u03B5\u03C1\u03BF\u03C5\u03C2
    \u03BC\u03B1\u03B8\u03B7\u03C4\u03AD\u03C2 \u03B1\u03C0\u03CC \u03C4\u03B7\u03BD
    \u03BF\u03BC\u03AC\u03B4\u03B1 \u0393, \u03BF\u03C0\u03CC\u03C4\u03B5 \u03B1\u03C0\u03CC
    \u03C4\u03BF \u03C3\u03CD\u03BD\u03BF\u03BB\u03BF \u03C4\u03C9\u03BD 55 \u03BC\u03B1\u03B8\u03B7\u03C4\u03CE\u03BD
    \u03C0\u03BF\u03C5 \u03B1\u03BD\u03AE\u03BA\u03BF\u03C5\u03BD \u03C3\u03C4\u03B9\u03C2
    \u03B4\u03CD\u03BF \u03BF\u03BC\u03AC\u03B4\u03B5\u03C2, \u03B3\u03BD\u03C9\u03C1\u03AF\u03B6\u03BF\u03C5\u03BC\u03B5
    \u03CC\u03C4\u03B9 \u03C4\u03BF\u03C5\u03BB\u03AC\u03C7\u03B9\u03C3\u03C4\u03BF\u03BD
    15 \u03B1\u03BD\u03AE\u03BA\u03BF\u03C5\u03BD \u03C3\u03C4\u03B7\u03BD \u03BF\u03BC\u03AC\u03B4\u03B1
    \u0392, \u03BF\u03C0\u03CC\u03C4\u03B5 55 - 15 = 40 \u03BC\u03B1\u03B8\u03B7\u03C4\u03AD\u03C2
    \u03BA\u03B1\u03C4\u03B1\u03BD\u03AD\u03BC\u03BF\u03BD\u03C4\u03B1\u03B9 \u03B5\u03BE\u03AF\u03C3\u03BF\u03C5
    \u03C3\u03C4\u03B9\u03C2 \u03B4\u03CD\u03BF \u03BF\u03BC\u03AC\u03B4\u03B5\u03C2.
    \u0395\u03C0\u03BF\u03BC\u03AD\u03BD\u03C9\u03C2, \u03B7 \u03BF\u03BC\u03AC\u03B4\u03B1
    \u0392 \u03AD\u03C7\u03B5\u03B9 40/2 = 20, 20 + 15 = 35 \u03BC\u03B1\u03B8\u03B7\u03C4\u03AD\u03C2
    \u03BA\u03B1\u03B9 \u03B7 \u03BF\u03BC\u03AC\u03B4\u03B1 \u0393 \u03AD\u03C7\u03B5\u03B9
    40/2 = 20 \u03BC\u03B1\u03B8\u03B7\u03C4\u03AD\u03C2!"
  - "\u0391\u03BD \u03B8\u03AD\u03BB\u03B1\u03BC\u03B5 \u03BD\u03B1 \u03B5\u03C0\u03B9\u03BB\u03AD\u03BE\u03BF\u03C5\u03BC\u03B5
    \u03BC\u03AF\u03B1 \u03B1\u03C0\u03CC \u03C4\u03B9\u03C2 \u03BB\u03AD\u03BE\u03B5\u03B9\u03C2
    \u03C0\u03BF\u03C5 \u03C4\u03B1\u03B9\u03C1\u03B9\u03AC\u03B6\u03B5\u03B9 \u03BB\u03B9\u03B3\u03CC\u03C4\u03B5\u03C1\u03BF
    \u03BC\u03B5 \u03C4\u03B9\u03C2 \u03C5\u03C0\u03CC\u03BB\u03BF\u03B9\u03C0\u03B5\u03C2,
    \u03B8\u03B1 \u03B1\u03C6\u03B1\u03B9\u03C1\u03BF\u03CD\u03C3\u03B1\u03BC\u03B5
    \u03C4\u03B7 \u03BB\u03AD\u03BE\u03B7 \"\u0394\u03B5\u03BA\u03AD\u03BC\u03B2\u03C1\u03B9\u03BF\u03C2\",
    \u03BA\u03B1\u03B8\u03CE\u03C2 \u03BF \u03A3\u03B5\u03C0\u03C4\u03AD\u03BC\u03B2\u03C1\u03B9\u03BF\u03C2,
    \u03BF \u039F\u03BA\u03C4\u03CE\u03B2\u03C1\u03B9\u03BF\u03C2 \u03BA\u03B1\u03B9
    \u03BF \u039D\u03BF\u03AD\u03BC\u03B2\u03C1\u03B9\u03BF\u03C2 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03CC\u03BB\u03BF\u03B9 \u03BF\u03B9 \u03BC\u03AE\u03BD\u03B5\u03C2 \u03C4\u03BF\u03C5
    \u03C6\u03B8\u03B9\u03BD\u03BF\u03C0\u03CE\u03C1\u03BF\u03C5, \u03B5\u03BD\u03CE
    \u03BF \u0394\u03B5\u03BA\u03AD\u03BC\u03B2\u03C1\u03B9\u03BF\u03C2 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03AD\u03BD\u03B1\u03C2 \u03B1\u03C0\u03CC \u03C4\u03BF\u03C5\u03C2 \u03C7\u03B5\u03B9\u03BC\u03B5\u03C1\u03B9\u03BD\u03BF\u03CD\u03C2
    \u03BC\u03AE\u03BD\u03B5\u03C2."
  - "\u03A3\u03BA\u03B5\u03C6\u03C4\u03B5\u03AF\u03C4\u03B5 \u03C4\u03BF \u03B5\u03C0\u03AF\u03C0\u03B5\u03B4\u03BF
    \u03C0\u03C1\u03BF\u03C4\u03B5\u03C1\u03B1\u03B9\u03CC\u03C4\u03B7\u03C4\u03B1\u03C2
    \u03C3\u03C4\u03B1 \u03BC\u03B1\u03B8\u03B7\u03BC\u03B1\u03C4\u03B9\u03BA\u03AC.
    \u03A4\u03B9 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1 \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B5\u03AF
    \u03C0\u03C1\u03CE\u03C4\u03B1; \u03A7\u03C1\u03B7\u03C3\u03B9\u03BC\u03BF\u03C0\u03BF\u03B9\u03CE\u03BD\u03C4\u03B1\u03C2
    \u03C4\u03BF \u03B5\u03C0\u03AF\u03C0\u03B5\u03B4\u03BF \u03C0\u03C1\u03BF\u03C4\u03B5\u03C1\u03B1\u03B9\u03CC\u03C4\u03B7\u03C4\u03B1\u03C2,
    \u03C0\u03C1\u03BF\u03C3\u03C0\u03B1\u03B8\u03AE\u03C3\u03C4\u03B5 \u03BD\u03B1
    \u03C3\u03C5\u03BC\u03C0\u03B5\u03C1\u03B9\u03BB\u03AC\u03B2\u03B5\u03C4\u03B5
    \u03CC\u03BB\u03BF\u03C5\u03C2 \u03C4\u03BF\u03C5\u03C2 \u03C0\u03B1\u03C1\u03AC\u03B3\u03BF\u03BD\u03C4\u03B5\u03C2
    \u03C3\u03C4\u03B7\u03BD \u03B1\u03C1\u03B9\u03C3\u03C4\u03B5\u03C1\u03AE \u03C0\u03BB\u03B5\u03C5\u03C1\u03AC
    \u03C4\u03B7\u03C2 \u03B5\u03BE\u03AF\u03C3\u03C9\u03C3\u03B7\u03C2. \u03A0\u03C1\u03CC\u03BA\u03B5\u03B9\u03C4\u03B1\u03B9
    \u03B3\u03B9\u03B1 \u03C4\u03B5\u03C4\u03C1\u03B1\u03B3\u03C9\u03BD\u03B9\u03BA\u03AE
    \u03C0\u03BF\u03BB\u03C5\u03C9\u03BD\u03C5\u03BC\u03B9\u03BA\u03AE \u03C3\u03C5\u03BD\u03AC\u03C1\u03C4\u03B7\u03C3\u03B7.
    \u0388\u03C4\u03C3\u03B9, \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF\u03C4\u03B5 \u03BD\u03B1
    \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AE\u03C3\u03B5\u03C4\u03B5 \u03C4\u03BF\u03C5\u03C2
    \u03BA\u03B1\u03BD\u03CC\u03BD\u03B5\u03C2 \u03B5\u03C0\u03AF\u03BB\u03C5\u03C3\u03B7\u03C2
    \u03B1\u03C5\u03C4\u03BF\u03CD \u03C4\u03BF\u03C5 \u03C4\u03CD\u03C0\u03BF\u03C5
    \u03C4\u03CD\u03C0\u03BF\u03C5 \u03C4\u03CD\u03C0\u03BF\u03C5 (X=[-b+\u221A(b2
    - 4ac)]/2a). \u0397 \u03C3\u03C9\u03C3\u03C4\u03AE \u03B1\u03C0\u03AC\u03BD\u03C4\u03B7\u03C3\u03B7
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B7 \u03B4: X-25=X^2+5X-24 >> -X^2-4X-1=0 >>
    \u03A4\u03CE\u03C1\u03B1, a=-1, b=-4, c=-1 >> X=[+4+\u221A(-16 - 4)]/-2) >> X=4+\u221A12/-2
    >> X=2-\u221A3= -0,268"
  - "\u03A3\u03BA\u03B5\u03C6\u03C4\u03B5\u03AF\u03C4\u03B5 \u03C4\u03BF \u03BC\u03BF\u03C4\u03AF\u03B2\u03BF
    \u03BA\u03B1\u03B9 \u03C4\u03BF \u03BA\u03B5\u03BD\u03CC \u03BC\u03B5\u03C4\u03B1\u03BE\u03CD
    \u03C4\u03BF\u03C5 \u03B1\u03BB\u03C6\u03B1\u03B2\u03AE\u03C4\u03BF\u03C5. \u039E\u03B5\u03BA\u03AF\u03BD\u03B7\u03C3\u03B5
    \u03B1\u03C0\u03CC \u03C4\u03BF \u0391 \u03BA\u03B1\u03B9 \u03AD\u03C6\u03C4\u03B1\u03C3\u03B5
    \u03C3\u03C4\u03BF \u0395. \u03A0\u03CC\u03C3\u03B1 \u03B3\u03C1\u03AC\u03BC\u03BC\u03B1\u03C4\u03B1
    \u03C0\u03B1\u03C1\u03B1\u03BB\u03B5\u03AF\u03C6\u03B8\u03B7\u03BA\u03B1\u03BD;
    \u039C\u03B5\u03C4\u03C1\u03AE\u03C3\u03C4\u03B5 \u03C4\u03B1. \u03A3\u03C4\u03B7
    \u03C3\u03C5\u03BD\u03AD\u03C7\u03B5\u03B9\u03B1, \u03B5\u03BB\u03AD\u03B3\u03BE\u03C4\u03B5
    \u03C0\u03CC\u03C3\u03B1 \u03B3\u03C1\u03AC\u03BC\u03BC\u03B1\u03C4\u03B1 \u03C0\u03B1\u03C1\u03B1\u03BB\u03B5\u03AF\u03C6\u03B8\u03B7\u03BA\u03B1\u03BD
    \u03B1\u03C0\u03CC \u03C4\u03BF \u0395 \u03AD\u03C9\u03C2 \u03C4\u03BF \u0399
    \u03BA\u03B1\u03B9 \u03C0\u03CC\u03C3\u03B1 \u03B1\u03C0\u03CC \u03C4\u03BF \u0399
    \u03AD\u03C9\u03C2 \u03C4\u03BF \u039C; \u0395\u03AF\u03BD\u03B1\u03B9 \u03BF
    \u03AF\u03B4\u03B9\u03BF\u03C2 \u03B1\u03C1\u03B9\u03B8\u03BC\u03CC\u03C2; \u03A4\u03CE\u03C1\u03B1
    \u03B5\u03C6\u03B1\u03C1\u03BC\u03CC\u03C3\u03C4\u03B5 \u03C4\u03BF\u03BD \u03B1\u03C1\u03B9\u03B8\u03BC\u03CC
    \u03C0\u03BF\u03C5 \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03AF\u03C3\u03B1\u03C4\u03B5
    \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03B1\u03C0\u03B1\u03BD\u03C4\u03AE\u03C3\u03B5\u03C4\u03B5
    \u03C3\u03C4\u03BF \u03B5\u03C1\u03CE\u03C4\u03B7\u03BC\u03B1. \u0397 \u03C3\u03C9\u03C3\u03C4\u03AE
    \u03B1\u03C0\u03AC\u03BD\u03C4\u03B7\u03C3\u03B7 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03C4\u03BF \u0393."
  - "\u03A7\u03C1\u03B7\u03C3\u03B9\u03BC\u03BF\u03C0\u03BF\u03B9\u03AE\u03C3\u03C4\u03B5
    \u03C4\u03BF\u03BD \u03C0\u03BF\u03BB\u03BB\u03B1\u03C0\u03BB\u03B1\u03C3\u03B9\u03B1\u03C3\u03BC\u03CC
    \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03B4\u03B9\u03B1\u03BD\u03B5\u03AF\u03BC\u03B5\u03C4\u03B5
    \u03C4\u03B7 \u03BC\u03B5\u03C1\u03AF\u03B4\u03B1 \u03C3\u03C4\u03B9\u03C2 \u03B1\u03B3\u03BA\u03CD\u03BB\u03B5\u03C2
    \u03BA\u03B1\u03B9 \u03B1\u03C6\u03B1\u03B9\u03C1\u03AD\u03C3\u03C4\u03B5 \u03B1\u03C5\u03C4\u03AD\u03C2
    \u03C0\u03BF\u03C5 \u03AD\u03C7\u03BF\u03C5\u03BD \u03C4\u03B9\u03C2 \u03AF\u03B4\u03B9\u03B5\u03C2
    \u03BC\u03B5\u03C4\u03B1\u03B2\u03BB\u03B7\u03C4\u03AD\u03C2. \u0397 \u03C3\u03C9\u03C3\u03C4\u03AE
    \u03B1\u03C0\u03AC\u03BD\u03C4\u03B7\u03C3\u03B7 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03B7 \u03B2: 6x(x + 3y -1) - 6x\u02C62 - 8xy  = 6x\u02C62 + 18xy - 6x - 6x\u02C62
