    \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC \u03C0\u03BF\u03C5 \u03B4\u03AF\u03BD\u03B5\u03C4\u03B1\u03B9:"
  - "\u0393\u03B9\u03B1 \u03BD\u03B1 \u03B4\u03B9\u03B4\u03AC\u03BE\u03B5\u03C4\u03B5
    \u03C3\u03C4\u03BF \u03C1\u03BF\u03BC\u03C0\u03CC\u03C4 \u03C0\u03CE\u03C2 \u03BD\u03B1
    \u03BA\u03B1\u03B8\u03B1\u03C1\u03AF\u03B6\u03B5\u03B9 \u03AD\u03BD\u03B1 \u03B4\u03C9\u03BC\u03AC\u03C4\u03B9\u03BF,
    \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1 \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AE\u03C3\u03B5\u03C4\u03B5
    \u03C4\u03B1 \u03C0\u03B1\u03C1\u03B1\u03BA\u03AC\u03C4\u03C9 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1
    \u03BC\u03B5 \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC \u03C0\u03BF\u03C5 \u03B4\u03AF\u03BD\u03B5\u03C4\u03B1\u03B9:"
  - "\u0397 \u03BB\u03AF\u03C3\u03C4\u03B1 \u03C5\u03C0\u03BF\u03C7\u03C1\u03B5\u03CE\u03C3\u03B5\u03C9\u03BD
    \u03B3\u03B9\u03B1 \u03C4\u03BF\u03BD \u03BA\u03B1\u03B8\u03B1\u03C1\u03B9\u03C3\u03BC\u03CC
    \u03B5\u03BD\u03CC\u03C2 \u03B4\u03C9\u03BC\u03B1\u03C4\u03AF\u03BF\u03C5 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03C0\u03B5\u03C1\u03B9\u03BB\u03B1\u03BC\u03B2\u03AC\u03BD\u03B5\u03B9
    \u03C4\u03B9\u03C2 \u03B1\u03BA\u03CC\u03BB\u03BF\u03C5\u03B8\u03B5\u03C2 \u03C0\u03BB\u03B7\u03C1\u03BF\u03C6\u03BF\u03C1\u03AF\u03B5\u03C2:"
  - "\u03A7\u03C1\u03B5\u03B9\u03AC\u03B6\u03BF\u03BD\u03C4\u03B1\u03B9 2 \u03CE\u03C1\u03B5\u03C2
    \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03C6\u03BF\u03C1\u03C4\u03B9\u03C3\u03C4\u03B5\u03AF
    \u03C0\u03BB\u03AE\u03C1\u03C9\u03C2 \u03AD\u03BD\u03B1 \u03BA\u03B9\u03BD\u03B7\u03C4\u03CC
    \u03C4\u03B7\u03BB\u03AD\u03C6\u03C9\u03BD\u03BF. \u03A0\u03BF\u03B9\u03BF\u03C2
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03BF \u03C3\u03C5\u03BD\u03C4\u03BF\u03BC\u03CC\u03C4\u03B5\u03C1\u03BF\u03C2
    \u03C7\u03C1\u03CC\u03BD\u03BF\u03C2 \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03C6\u03BF\u03C1\u03C4\u03AF\u03C3\u03B5\u03C4\u03B5
    \u03C0\u03BB\u03AE\u03C1\u03C9\u03C2 3 \u03BA\u03B9\u03BD\u03B7\u03C4\u03AC \u03C4\u03B7\u03BB\u03AD\u03C6\u03C9\u03BD\u03B1
    \u03BC\u03B5 2 \u03C6\u03BF\u03C1\u03C4\u03B9\u03C3\u03C4\u03AD\u03C2;"
  - "\u038C\u03C4\u03B1\u03BD \u03B3\u03C1\u03AC\u03C6\u03B5\u03C4\u03B5 \u03AD\u03BD\u03B1
    \u03C0\u03C1\u03CC\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1 \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03C4\u03C5\u03B1\u03BA\u03CE\u03BD
    \u03B1\u03B3\u03BF\u03C1\u03CE\u03BD, \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1
    \u03B5\u03C0\u03B9\u03BB\u03AD\u03BE\u03B5\u03C4\u03B5 \u03C4\u03B7 \u03C3\u03C9\u03C3\u03C4\u03AE
    \u03C3\u03B5\u03B9\u03C1\u03AC \u03C4\u03C9\u03BD \u03B5\u03C1\u03B3\u03B1\u03C3\u03B9\u03CE\u03BD
    \u03C0\u03BF\u03C5 \u03B8\u03B1 \u03B5\u03BA\u03C4\u03B5\u03BB\u03AD\u03C3\u03B5\u03B9:"
  - "\u0393\u03B9\u03B1 \u03BD\u03B1 \u03C3\u03C7\u03B5\u03B4\u03B9\u03AC\u03C3\u03B5\u03C4\u03B5
    \u03AD\u03BD\u03B1 \u03C0\u03C1\u03AC\u03C3\u03B9\u03BD\u03BF \u03C4\u03B5\u03C4\u03C1\u03AC\u03B3\u03C9\u03BD\u03BF
    150 \u03C0\u03AF\u03BE\u03B5\u03BB x 150 \u03C0\u03AF\u03BE\u03B5\u03BB, \u03C4\u03BF
    \u03C0\u03C1\u03CC\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AE\u03C3\u03B5\u03B9
    \u03C4\u03B1 \u03B1\u03BA\u03CC\u03BB\u03BF\u03C5\u03B8\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1
    \u03BC\u03B5 \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC \u03C0\u03BF\u03C5 \u03B4\u03AF\u03BD\u03B5\u03C4\u03B1\u03B9:"
  - "\u03A0\u03BF\u03B9\u03BF \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C4\u03BF \u03C3\u03C6\u03AC\u03BB\u03BC\u03B1
    \u03C3\u03C4\u03BF \u03C0\u03B1\u03C1\u03B1\u03BA\u03AC\u03C4\u03C9 \u03C0\u03C1\u03CC\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1,
    \u03C4\u03BF \u03BF\u03C0\u03BF\u03AF\u03BF \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03C3\u03C7\u03B5\u03B4\u03B9\u03AC\u03C3\u03B5\u03B9 \u03AD\u03BD\u03B1
    \u03C4\u03B5\u03C4\u03C1\u03AC\u03B3\u03C9\u03BD\u03BF 100 \u03C0\u03AF\u03BE\u03B5\u03BB
    x 100 \u03C0\u03AF\u03BE\u03B5\u03BB \u03BC\u03AF\u03B1 \u03C6\u03BF\u03C1\u03AC,
    \u03BE\u03B5\u03BA\u03B9\u03BD\u03CE\u03BD\u03C4\u03B1\u03C2 \u03B1\u03C0\u03CC
    \u03C4\u03B7\u03BD \u03B5\u03C0\u03AC\u03BD\u03C9 \u03B1\u03C1\u03B9\u03C3\u03C4\u03B5\u03C1\u03AE
    \u03B3\u03C9\u03BD\u03AF\u03B1 \u03C0\u03C1\u03BF\u03C2 \u03C4\u03B1 \u03B4\u03B5\u03BE\u03B9\u03AC:
    \u03B5\u03C0\u03B1\u03BD\u03B1\u03BB\u03AC\u03B2\u03B5\u03C4\u03B5 4 \u03C6\u03BF\u03C1\u03AD\u03C2
    \u03C4\u03BF \u03BC\u03C0\u03BB\u03BF\u03BA \u03C3\u03B5 \u03C0\u03B1\u03C1\u03AD\u03BD\u03B8\u03B5\u03C3\u03B7
    [\u03BC\u03B5\u03C4\u03B1\u03BA\u03AF\u03BD\u03B7\u03C3\u03B7 100 \u03C0\u03AF\u03BE\u03B5\u03BB],
    \u03C3\u03C4\u03C1\u03AF\u03C8\u03C4\u03B5 \u03B4\u03B5\u03BE\u03B9\u03AC 90
    \u03BC\u03BF\u03AF\u03C1\u03B5\u03C2."
  - "\u03A0\u03CE\u03C2 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BF \u03B9\u03C0\u03C0\u03CC\u03C4\u03B7\u03C2
    (\u03BC\u03B9\u03B1 \u03C6\u03B9\u03B3\u03BF\u03CD\u03C1\u03B1 \u03C3\u03BA\u03B1\u03BA\u03B9\u03BF\u03CD
    \u03C0\u03BF\u03C5 \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF \u03BD\u03B1 \u03BC\u03B5\u03C4\u03B1\u03BA\u03B9\u03BD\u03AE\u03B8\u03B5\u03B9
    \u03B4\u03CD\u03BF \u03C0\u03B5\u03B4\u03AF\u03B1 \u03BF\u03C1\u03B9\u03B6\u03CC\u03BD\u03C4\u03B9\u03B1
    \u03BA\u03B1\u03B9 \u03AD\u03BD\u03B1 \u03BA\u03AC\u03B8\u03B5\u03C4\u03B1, \u03AE
    \u03B4\u03CD\u03BF \u03BA\u03AC\u03B8\u03B5\u03C4\u03B1 \u03BA\u03B1\u03B9 \u03AD\u03BD\u03B1
    \u03BF\u03C1\u03B9\u03B6\u03CC\u03BD\u03C4\u03B9\u03B1) \u03B1\u03C0\u03CC \u03C4\u03BF
    \u03BC\u03B5\u03C3\u03B1\u03AF\u03BF \u03C0\u03B5\u03B4\u03AF\u03BF \u03C4\u03B7\u03C2
    \u03C3\u03B1\u03BD\u03AF\u03B4\u03B1\u03C2 5x5 \u03C3\u03C4\u03B7\u03BD \u03B5\u03C0\u03AC\u03BD\u03C9
    \u03B4\u03B5\u03BE\u03B9\u03AC \u03B3\u03C9\u03BD\u03AF\u03B1 \u03C4\u03BF\u03C5
    \u03C4\u03B1\u03BC\u03C0\u03BB\u03CC;"
  - "\u0397 \u03C3\u03BA\u03B1\u03BA\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE \u03C6\u03B9\u03B3\u03BF\u03CD\u03C1\u03B1
    - \u03BF \u03B9\u03C0\u03C0\u03CC\u03C4\u03B7\u03C2 \u03B2\u03C1\u03AF\u03C3\u03BA\u03B5\u03C4\u03B1\u03B9
    \u03C3\u03C4\u03BF \u03BC\u03B5\u03C3\u03B1\u03AF\u03BF \u03C0\u03B5\u03B4\u03AF\u03BF
    5x5. \u039F \u03B9\u03C0\u03C0\u03CC\u03C4\u03B7\u03C2 \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF
    \u03BD\u03B1 \u03BC\u03B5\u03C4\u03B1\u03BA\u03B9\u03BD\u03AE\u03C3\u03B5\u03B9
    \u03B4\u03CD\u03BF \u03C0\u03B5\u03B4\u03AF\u03B1 \u03BF\u03C1\u03B9\u03B6\u03CC\u03BD\u03C4\u03B9\u03B1
    \u03BA\u03B1\u03B9 \u03AD\u03BD\u03B1 \u03BA\u03AC\u03B8\u03B5\u03C4\u03B1 \u03AE
    \u03B4\u03CD\u03BF \u03BA\u03AC\u03B8\u03B5\u03C4\u03B1 \u03BA\u03B1\u03B9 \u03AD\u03BD\u03B1
    \u03BF\u03C1\u03B9\u03B6\u03CC\u03BD\u03C4\u03B9\u03B1. \u03A0\u03BF\u03B9\u03BF\u03C2
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03BF \u03BC\u03B9\u03BA\u03C1\u03CC\u03C4\u03B5\u03C1\u03BF\u03C2
    \u03B1\u03C1\u03B9\u03B8\u03BC\u03CC\u03C2 \u03BA\u03B9\u03BD\u03AE\u03C3\u03B5\u03C9\u03BD
    \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03C6\u03C4\u03AC\u03C3\u03B5\u03C4\u03B5 \u03C3\u03B5
    \u03BC\u03AF\u03B1 \u03B1\u03C0\u03CC \u03C4\u03B9\u03C2 \u03B3\u03C9\u03BD\u03AF\u03B5\u03C2;"
  - "\u0393\u03B9\u03B1 \u03BD\u03B1 \u03B5\u03C4\u03BF\u03B9\u03BC\u03AC\u03C3\u03B5\u03C4\u03B5
    \u03BC\u03B9\u03B1 \u03BA\u03B1\u03BB\u03AE \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE
    \u03B3\u03B9\u03B1 \u03BA\u03AD\u03B9\u03BA, \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AE\u03C3\u03C4\u03B5
    \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03BC\u03B5 \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC
    \u03C0\u03BF\u03C5 \u03B4\u03AF\u03BD\u03BF\u03BD\u03C4\u03B1\u03B9:"
  - "\u0393\u03B9\u03B1 \u03BD\u03B1 \u03B4\u03B5\u03AF\u03C4\u03B5 \u03AD\u03BD\u03B1
    \u03B1\u03BD\u03C4\u03B9\u03BA\u03B5\u03AF\u03BC\u03B5\u03BD\u03BF \u03C3\u03B5
    \u03BC\u03B9\u03B1 \u03C3\u03C4\u03B5\u03C1\u03B5\u03BF\u03B3\u03C1\u03B1\u03C6\u03B9\u03BA\u03AE
    \u03B5\u03B9\u03BA\u03CC\u03BD\u03B1 (\u03B3\u03B9\u03B1 \u03BD\u03B1 \u03B4\u03B5\u03AF\u03C4\u03B5
    \u03AD\u03BD\u03B1 \u03B4\u03B9\u03C3\u03B4\u03B9\u03AC\u03C3\u03C4\u03B1\u03C4\u03BF
    \u03B3\u03C1\u03B1\u03C6\u03B9\u03BA\u03CC \u03C3\u03B5 \u03C4\u03C1\u03B5\u03B9\u03C2
    \u03B4\u03B9\u03B1\u03C3\u03C4\u03AC\u03C3\u03B5\u03B9\u03C2, \u03CC\u03C0\u03C9\u03C2
    \u03C6\u03B1\u03AF\u03BD\u03B5\u03C4\u03B1\u03B9 \u03B1\u03C0\u03CC \u03BC\u03B9\u03B1
    \u03C0\u03C1\u03BF\u03BF\u03C0\u03C4\u03B9\u03BA\u03AE), \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03BA\u03AC\u03BD\u03B5\u03C4\u03B5 \u03C4\u03B1 \u03B5\u03BE\u03AE\u03C2
    \u03BC\u03B5 \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC \u03C0\u03BF\u03C5 \u03B4\u03AF\u03BD\u03B5\u03C4\u03B1\u03B9:"
  Question_Explanation:
  - "\u0397 \u03A5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE
    \u03A3\u03BA\u03AD\u03C8\u03B7 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03BC\u03B9\u03B1
    \u03B5\u03C0\u03B9\u03C3\u03C4\u03B7\u03BC\u03BF\u03BD\u03B9\u03BA\u03AE \u03C0\u03B5\u03C1\u03B9\u03BF\u03C7\u03AE
    \u03BC\u03B5 \u03C0\u03BF\u03BB\u03BB\u03BF\u03CD\u03C2 \u03BF\u03C1\u03B9\u03C3\u03BC\u03BF\u03CD\u03C2.
    \u0388\u03BD\u03B1 \u03BA\u03BF\u03B9\u03BD\u03CC \u03C3\u03C4\u03BF\u03B9\u03C7\u03B5\u03AF\u03BF
    \u03C0\u03BF\u03C5 \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5 \u03BD\u03B1
    \u03B2\u03C1\u03BF\u03CD\u03BC\u03B5 \u03C3\u03C4\u03BF\u03C5\u03C2 \u03BF\u03C1\u03B9\u03C3\u03BC\u03BF\u03CD\u03C2
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B7 \u03C3\u03CD\u03BD\u03B4\u03B5\u03C3\u03B7
    \u03BC\u03B5 \u03C4\u03B5\u03C7\u03BD\u03B9\u03BA\u03AD\u03C2 \u03B5\u03C0\u03AF\u03BB\u03C5\u03C3\u03B7\u03C2
    \u03C0\u03C1\u03BF\u03B2\u03BB\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD. "
  - "\u0397 \u03A5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE
    \u03A3\u03BA\u03AD\u03C8\u03B7 \u03BC\u03B1\u03C2 \u03C5\u03C0\u03BF\u03C3\u03C4\u03B7\u03C1\u03AF\u03B6\u03B5\u03B9
    \u03C4\u03B7 \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03B1\u03C3\u03AF\u03B1 \u03B5\u03C0\u03AF\u03BB\u03C5\u03C3\u03B7\u03C2
    \u03C0\u03C1\u03BF\u03B2\u03BB\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD, \u03B3\u03B9\u03B1
    \u03BD\u03B1 \u03BA\u03B1\u03C4\u03B1\u03BD\u03BF\u03AE\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03BF \u03C0\u03C1\u03CC\u03B2\u03BB\u03B7\u03BC\u03B1 \u03BA\u03B1\u03B9
    \u03C0\u03C1\u03BF\u03C3\u03C0\u03B1\u03B8\u03AE\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03BD\u03B1 \u03C4\u03BF \u03BB\u03CD\u03C3\u03BF\u03C5\u03BC\u03B5 \u03B1\u03BD\u03B1\u03C0\u03C4\u03CD\u03C3\u03C3\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03BB\u03CD\u03C3\u03B5\u03B9\u03C2. "
  - "\u0397 \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE
    \u03C3\u03BA\u03AD\u03C8\u03B7 \u03BA\u03B1\u03B9 \u03B7 \u03BA\u03B1\u03C4\u03B1\u03C3\u03BA\u03B5\u03C5\u03AE
    \u03BA\u03CE\u03B4\u03B9\u03BA\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C3\u03C7\u03B5\u03C4\u03B9\u03BA\u03AD\u03C2
    \u03AD\u03BD\u03BD\u03BF\u03B9\u03B5\u03C2. \u0397 \u03BA\u03B1\u03C4\u03B1\u03C3\u03BA\u03B5\u03C5\u03AE
    \u03BA\u03CE\u03B4\u03B9\u03BA\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B7 \u03BC\u03B5\u03C4\u03AC\u03C6\u03C1\u03B1\u03C3\u03B7
    \u03C4\u03B7\u03C2 \u03BB\u03BF\u03B3\u03B9\u03BA\u03AE\u03C2 \u03AE \u03C4\u03C9\u03BD
    \u03C3\u03C5\u03BB\u03BB\u03BF\u03B3\u03B9\u03C3\u03BC\u03CE\u03BD \u03C3\u03B5
    \u03BC\u03B9\u03B1 \u03B3\u03BB\u03CE\u03C3\u03C3\u03B1 \u03C0\u03C1\u03BF\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1\u03C4\u03B9\u03C3\u03BC\u03BF\u03CD."
  - "\u039F \u03C0\u03C1\u03BF\u03C3\u03B5\u03BA\u03C4\u03B9\u03BA\u03CC\u03C2 \u03C3\u03C7\u03B5\u03B4\u03B9\u03B1\u03C3\u03BC\u03CC\u03C2
    \u03B1\u03C0\u03BF\u03C4\u03B5\u03BB\u03B5\u03AF \u03B4\u03B5\u03BE\u03B9\u03CC\u03C4\u03B7\u03C4\u03B1
    \u03C4\u03B7\u03C2 \u03A5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE\u03C2
    \u03A3\u03BA\u03AD\u03C8\u03B7\u03C2, \u03C4\u03B7 \u03B4\u03B5\u03BE\u03B9\u03CC\u03C4\u03B7\u03C4\u03B1
    \u03C4\u03B7\u03C2 \u0391\u03BB\u03B3\u03BF\u03C1\u03B9\u03B8\u03BC\u03B9\u03BA\u03AE\u03C2
    \u03A3\u03BA\u03AD\u03C8\u03B7\u03C2. "
  - "\u0397 \u0391\u03C0\u03BF\u03C3\u03CD\u03BD\u03B8\u03B5\u03C3\u03B7 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03B7 \u03B4\u03B5\u03BE\u03B9\u03CC\u03C4\u03B7\u03C4\u03B1 \u03C4\u03B7\u03C2
    \u03B4\u03B9\u03AC\u03C3\u03C0\u03B1\u03C3\u03B7\u03C2 \u03B5\u03BD\u03CC\u03C2
    \u03C0\u03C1\u03BF\u03B2\u03BB\u03AE\u03BC\u03B1\u03C4\u03BF\u03C2 \u03C3\u03B5
    \u03BC\u03B9\u03BA\u03C1\u03CC\u03C4\u03B5\u03C1\u03B1 \u03C3\u03C4\u03BF\u03B9\u03C7\u03B5\u03AF\u03B1."
  - "\u0397 \u0391\u03BB\u03B3\u03BF\u03C1\u03B9\u03B8\u03BC\u03B9\u03BA\u03AE \u03A3\u03BA\u03AD\u03C8\u03B7
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03BF \u03BF\u03C1\u03B9\u03C3\u03BC\u03CC\u03C2
    \u03C3\u03C5\u03B3\u03BA\u03B5\u03BA\u03C1\u03B9\u03BC\u03AD\u03BD\u03C9\u03BD
    \u03B2\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD \u03B3\u03B9\u03B1 \u03C4\u03BF\u03BD
    \u03C3\u03C7\u03B5\u03B4\u03B9\u03B1\u03C3\u03BC\u03CC \u03BC\u03B9\u03B1\u03C2
    \u03BB\u03CD\u03C3\u03B7\u03C2. "
  - "\u039A\u03B1\u03B8\u03CE\u03C2 \u03C4\u03BF \u03BC\u03BF\u03C4\u03AF\u03B2\u03BF
    \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03B5\u03AF \u03C4\u03B7\u03BD \u03C0\u03C1\u03BF\u03C3\u03B8\u03AE\u03BA\u03B7
    5 \u03BF \u03B5\u03C0\u03CC\u03BC\u03B5\u03BD\u03BF\u03C2 \u03B1\u03C1\u03B9\u03B8\u03BC\u03CC\u03C2
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C4\u03BF 40."
  - "\u0397 \u0391\u03C0\u03BF\u03C3\u03CD\u03BD\u03B8\u03B5\u03C3\u03B7 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03B7 \u03B4\u03B5\u03BE\u03B9\u03CC\u03C4\u03B7\u03C4\u03B1 \u03C4\u03B7\u03C2
    \u03B4\u03B9\u03AC\u03C3\u03C0\u03B1\u03C3\u03B7\u03C2 \u03B5\u03BD\u03CC\u03C2
    \u03C0\u03B5\u03C1\u03AF\u03C0\u03BB\u03BF\u03BA\u03BF\u03C5 \u03C0\u03C1\u03BF\u03B2\u03BB\u03AE\u03BC\u03B1\u03C4\u03BF\u03C2
    \u03C3\u03B5 \u03BC\u03B9\u03BA\u03C1\u03CC\u03C4\u03B5\u03C1\u03B1 \u03C3\u03C4\u03BF\u03B9\u03C7\u03B5\u03AF\u03B1."
  - "\u039C\u03AD\u03C3\u03C9 \u03C4\u03B7\u03C2 \u03B1\u03BD\u03B1\u03B3\u03BD\u03CE\u03C1\u03B9\u03C3\u03B7\u03C2
    \u03C0\u03C1\u03BF\u03C4\u03CD\u03C0\u03C9\u03BD, \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5
    \u03BD\u03B1 \u03B5\u03BD\u03C4\u03BF\u03C0\u03AF\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03BF\u03BC\u03BF\u03B9\u03CC\u03C4\u03B7\u03C4\u03B5\u03C2 \u03AE \u03B1\u03BD\u03C4\u03B9\u03B8\u03AD\u03C3\u03B5\u03B9\u03C2
    \u03BA\u03B1\u03C4\u03AC \u03C4\u03B7 \u03B4\u03B9\u03AC\u03C1\u03BA\u03B5\u03B9\u03B1
    \u03BC\u03B9\u03B1\u03C2 \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03B1\u03C3\u03AF\u03B1\u03C2
    \u03B5\u03C0\u03AF\u03BB\u03C5\u03C3\u03B7\u03C2 \u03C0\u03C1\u03BF\u03B2\u03BB\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD
    \u03BA\u03B1\u03B9 \u03B5\u03C0\u03AF\u03C3\u03B7\u03C2 \u03BD\u03B1 \u03B4\u03B7\u03BC\u03B9\u03BF\u03C5\u03C1\u03B3\u03AE\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03BC\u03B9\u03B1 \u03BA\u03B1\u03C4\u03B1\u03C3\u03BA\u03B5\u03C5\u03AE \u03C9\u03C2
    \u03B4\u03B9\u03B1\u03B4\u03C1\u03BF\u03BC\u03AE \u03B3\u03B9\u03B1 \u03C4\u03B7
    \u03BB\u03CD\u03C3\u03B7."
  - "\u0397 \u03B1\u03C6\u03B1\u03AF\u03C1\u03B5\u03C3\u03B7 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03B7 \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03B1\u03C3\u03AF\u03B1 \u03C6\u03B9\u03BB\u03C4\u03C1\u03B1\u03C1\u03AF\u03C3\u03BC\u03B1\u03C4\u03BF\u03C2
    \u03C4\u03C9\u03BD \u03C7\u03B1\u03C1\u03B1\u03BA\u03C4\u03B7\u03C1\u03B9\u03C3\u03C4\u03B9\u03BA\u03CE\u03BD
    \u03C4\u03C9\u03BD \u03BC\u03BF\u03C4\u03AF\u03B2\u03C9\u03BD \u03C0\u03BF\u03C5
    \u03B4\u03B5\u03BD \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C3\u03B7\u03BC\u03B1\u03BD\u03C4\u03B9\u03BA\u03AC
    \u03B3\u03B9\u03B1 \u03C4\u03B7\u03BD \u03B5\u03C0\u03AF\u03BB\u03C5\u03C3\u03B7
    \u03B5\u03BD\u03CC\u03C2 \u03C0\u03C1\u03BF\u03B2\u03BB\u03AE\u03BC\u03B1\u03C4\u03BF\u03C2."
  - "\u0397 \u03B1\u03BB\u03B3\u03BF\u03C1\u03B9\u03B8\u03BC\u03B9\u03BA\u03AE \u03C3\u03BA\u03AD\u03C8\u03B7
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03AD\u03BD\u03B1\u03C2 \u03C4\u03C1\u03CC\u03C0\u03BF\u03C2
    \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03C6\u03C4\u03AC\u03C3\u03B5\u03C4\u03B5 \u03C3\u03B5
    \u03BC\u03B9\u03B1 \u03BB\u03CD\u03C3\u03B7 \u03BC\u03AD\u03C3\u03C9 \u03C4\u03BF\u03C5
    \u03BF\u03C1\u03B9\u03C3\u03BC\u03BF\u03CD \u03B2\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD.
    \u0393\u03B9\u03B1 \u03C0\u03B1\u03C1\u03AC\u03B4\u03B5\u03B9\u03B3\u03BC\u03B1:
    \u03BF\u03B9 \u03BF\u03B4\u03B7\u03B3\u03AF\u03B5\u03C2 \u03BC\u03B9\u03B1\u03C2
    \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE\u03C2 \u03BC\u03B1\u03B3\u03B5\u03B9\u03C1\u03B9\u03BA\u03AE\u03C2."
  - "\u0397 \u03B3\u03B5\u03BD\u03AF\u03BA\u03B5\u03C5\u03C3\u03B7 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03B7 \u03B5\u03C0\u03AF\u03BB\u03C5\u03C3\u03B7 \u03BD\u03AD\u03C9\u03BD \u03C0\u03C1\u03BF\u03B2\u03BB\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD
    \u03BC\u03B5 \u03B2\u03AC\u03C3\u03B7 \u03C4\u03B1 \u03C0\u03C1\u03BF\u03B7\u03B3\u03BF\u03CD\u03BC\u03B5\u03BD\u03B1
    \u03C0\u03C1\u03BF\u03B2\u03BB\u03AE\u03BC\u03B1\u03C4\u03B1 \u03C0\u03BF\u03C5
    \u03AD\u03C7\u03BF\u03C5\u03BC\u03B5 \u03BB\u03CD\u03C3\u03B5\u03B9."
  - "\u0391\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AF\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC \u03BC\u03B5 \u03C4\u03B7\u03BD \u03BF\u03C0\u03BF\u03AF\u03B1
    \u03B5\u03BA\u03C4\u03B5\u03BB\u03BF\u03CD\u03BD\u03C4\u03B1\u03B9 \u03BF\u03B9
    \u03B5\u03BD\u03C4\u03BF\u03BB\u03AD\u03C2. \u0393\u03B9\u03B1 \u03C0\u03B1\u03C1\u03AC\u03B4\u03B5\u03B9\u03B3\u03BC\u03B1:
    \u03A3\u03C4\u03BF\u03BD \u03C0\u03C1\u03BF\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1\u03C4\u03B9\u03C3\u03BC\u03CC,
    \u03BF\u03B9 \u03B5\u03BD\u03C4\u03BF\u03BB\u03AD\u03C2 \u03B5\u03BA\u03C4\u03B5\u03BB\u03BF\u03CD\u03BD\u03C4\u03B1\u03B9
    \u03B7 \u03BC\u03AF\u03B1 \u03BC\u03B5\u03C4\u03AC \u03C4\u03B7\u03BD \u03AC\u03BB\u03BB\u03B7."
  - "\u039F \u03B5\u03BD\u03C4\u03BF\u03C0\u03B9\u03C3\u03BC\u03CC\u03C2 \u03C3\u03C6\u03B1\u03BB\u03BC\u03AC\u03C4\u03C9\u03BD
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B7 \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03B1\u03C3\u03AF\u03B1
    \u03C0\u03BF\u03C5 \u03C0\u03B5\u03C1\u03B9\u03BB\u03B1\u03BC\u03B2\u03AC\u03BD\u03B5\u03B9
    \u03C4\u03B7\u03BD \u03B5\u03CD\u03C1\u03B5\u03C3\u03B7 \u03BA\u03B1\u03B9 \u03C4\u03B7
    \u03B4\u03B9\u03CC\u03C1\u03B8\u03C9\u03C3\u03B7 \u03C3\u03C6\u03B1\u03BB\u03BC\u03AC\u03C4\u03C9\u03BD
    \u03B5\u03AC\u03BD \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03B4\u03B5\u03BD
    \u03BF\u03B4\u03B7\u03B3\u03AE\u03C3\u03BF\u03C5\u03BD \u03C3\u03C4\u03B7\u03BD
    \u03B5\u03C0\u03B9\u03B8\u03C5\u03BC\u03B7\u03C4\u03AE \u03BB\u03CD\u03C3\u03B7."
  - "\u039F\u03B9 \u03B2\u03B1\u03C3\u03B9\u03BA\u03AD\u03C2 \u03B9\u03BA\u03B1\u03BD\u03CC\u03C4\u03B7\u03C4\u03B5\u03C2
    \u03C4\u03B7\u03C2 \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE\u03C2
    \u03C3\u03BA\u03AD\u03C8\u03B7\u03C2 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B7 \u0391\u03C6\u03B1\u03AF\u03C1\u03B5\u03C3\u03B7,
    \u0391\u03BB\u03B3\u03BF\u03C1\u03B9\u03B8\u03BC\u03B9\u03BA\u03AE \u03A3\u03BA\u03AD\u03C8\u03B7,
    \u0391\u03BD\u03B1\u03B3\u03BD\u03CE\u03C1\u03B9\u03C3\u03B7 \u03A0\u03C1\u03BF\u03C4\u03CD\u03C0\u03C9\u03BD,
    \u0391\u03C0\u03BF\u03C3\u03CD\u03BD\u03B8\u03B5\u03C3\u03B7."
  - "\u039F\u03B9 \u03B2\u03B1\u03C3\u03B9\u03BA\u03AD\u03C2 \u03B9\u03BA\u03B1\u03BD\u03CC\u03C4\u03B7\u03C4\u03B5\u03C2
    \u03C4\u03B7\u03C2 \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE\u03C2
    \u03C3\u03BA\u03AD\u03C8\u03B7\u03C2 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B7 \u0391\u03C6\u03B1\u03AF\u03C1\u03B5\u03C3\u03B7,
    \u0391\u03BB\u03B3\u03BF\u03C1\u03B9\u03B8\u03BC\u03B9\u03BA\u03AE \u03A3\u03BA\u03AD\u03C8\u03B7,
    \u0391\u03BD\u03B1\u03B3\u03BD\u03CE\u03C1\u03B9\u03C3\u03B7 \u03A0\u03C1\u03BF\u03C4\u03CD\u03C0\u03C9\u03BD,
    \u0391\u03C0\u03BF\u03C3\u03CD\u03BD\u03B8\u03B5\u03C3\u03B7. \u0397 \u03B4\u03B7\u03BC\u03B9\u03BF\u03C5\u03C1\u03B3\u03B9\u03BA\u03CC\u03C4\u03B7\u03C4\u03B1
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B9\u03BA\u03B1\u03BD\u03CC\u03C4\u03B7\u03C4\u03B1
    \u03C4\u03BF\u03C5 21\u03BF\u03C5 \u03B1\u03B9\u03CE\u03BD\u03B1. "
  - "\u039A\u03B1\u03B8\u03CE\u03C2 \u03B7 \u0391\u03BB\u03B3\u03BF\u03C1\u03B9\u03B8\u03BC\u03B9\u03BA\u03AE
    \u03A3\u03BA\u03AD\u03C8\u03B7 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03BF \u03BF\u03C1\u03B9\u03C3\u03BC\u03CC\u03C2
    \u03C3\u03C5\u03B3\u03BA\u03B5\u03BA\u03C1\u03B9\u03BC\u03AD\u03BD\u03C9\u03BD
    \u03B2\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD \u03B3\u03B9\u03B1 \u03C4\u03BF\u03BD
    \u03C3\u03C7\u03B5\u03B4\u03B9\u03B1\u03C3\u03BC\u03CC \u03BC\u03B9\u03B1\u03C2
    \u03BB\u03CD\u03C3\u03B7\u03C2, \u03B7 \u03BA\u03B1\u03C4\u03B1\u03C3\u03C4\u03BA\u03B5\u03C5\u03AE
    \u03B5\u03BD\u03CC\u03C2 \u03B5\u03C0\u03AF\u03C0\u03BB\u03BF\u03C5 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03BC\u03B9\u03B1 \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03B1\u03C3\u03AF\u03B1
    \u03C0\u03BF\u03C5 \u03B1\u03C0\u03B1\u03B9\u03C4\u03B5\u03AF \u03C4\u03B7\u03BD
    \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AF\u03B1 \u03B2\u03AE\u03BC\u03B1
    \u03C0\u03C1\u03BF\u03C2 \u03B2\u03AE\u03BC\u03B1 \u03BF\u03B4\u03B7\u03B3\u03B9\u03CE\u03BD. "
  - "\"\u0397 \u03B5\u03BA\u03BC\u03AC\u03B8\u03B7\u03C3\u03B7 \u03B3\u03BB\u03C9\u03C3\u03C3\u03CE\u03BD
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C3\u03B1\u03BD \u03C4\u03B7\u03BD \u03C0\u03B1\u03C1\u03B1\u03C3\u03BA\u03B5\u03C5\u03AE
    \u03C4\u03C3\u03B1\u03B3\u03B9\u03BF\u03CD. \u0393\u03B9\u03B1 \u03BD\u03B1 \u03B3\u03C1\u03AC\u03C8\u03B5\u03C4\u03B5/\u03C0\u03B5\u03AF\u03C4\u03B5
    \u03BA\u03AC\u03C4\u03B9 \u03C3\u03CD\u03BC\u03C6\u03C9\u03BD\u03B1 \u03BC\u03B5
    \u03C4\u03B7 \u03B3\u03C1\u03B1\u03BC\u03BC\u03B1\u03C4\u03B9\u03BA\u03AE, \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03B3\u03BD\u03C9\u03C1\u03AF\u03B6\u03B5\u03C4\u03B5 \u03C4\u03BF\u03BD
    \u03BF\u03B4\u03B7\u03B3\u03CC \u03B3\u03BB\u03CE\u03C3\u03C3\u03B1\u03C2. \u0397
    \u03B3\u03C1\u03B1\u03BC\u03BC\u03B1\u03C4\u03B9\u03BA\u03AE \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF
    \u03BD\u03B1 \u03C3\u03C5\u03B3\u03BA\u03C1\u03B9\u03B8\u03B5\u03AF \u03BC\u03B5
    \u03BC\u03B9\u03B1 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE \u03BC\u03B1\u03B3\u03B5\u03B9\u03C1\u03B9\u03BA\u03AE\u03C2,
    \u03B5\u03BE\u03B7\u03B3\u03CE\u03BD\u03C4\u03B1\u03C2 \u03CC\u03BB\u03B1 \u03C4\u03B1
    \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03C0\u03BF\u03C5 \u03B1\u03C0\u03B1\u03B9\u03C4\u03BF\u03CD\u03BD\u03C4\u03B1\u03B9
    \u03B3\u03B9\u03B1 \u03C4\u03B7\u03BD \u03C0\u03C1\u03BF\u03B5\u03C4\u03BF\u03B9\u03BC\u03B1\u03C3\u03AF\u03B1
    \u03B5\u03BD\u03CC\u03C2 \u03C0\u03B9\u03AC\u03C4\u03BF\u03C5. \u0391\u03BB\u03BB\u03AC
    \u03C0\u03C1\u03CE\u03C4\u03B1 \u03B1\u03C0\u03CC \u03CC\u03BB\u03B1, \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03BE\u03AD\u03C1\u03BF\u03C5\u03BC\u03B5 \u03C4\u03B1 \u03C3\u03C5\u03C3\u03C4\u03B1\u03C4\u03B9\u03BA\u03AC!
    \u03A4\u03B1 \u03C3\u03C5\u03C3\u03C4\u03B1\u03C4\u03B9\u03BA\u03AC \u03C3\u03C4\u03B7
    \u03B3\u03C1\u03B1\u03BC\u03BC\u03B1\u03C4\u03B9\u03BA\u03AE \u03C4\u03B7\u03C2
    \u03B3\u03BB\u03CE\u03C3\u03C3\u03B1\u03C2 \u03B5\u03AF\u03BD\u03B1\u03B9, \u03B3\u03B9\u03B1
    \u03C0\u03B1\u03C1\u03AC\u03B4\u03B5\u03B9\u03B3\u03BC\u03B1, \u03C4\u03BF \u03B8\u03AD\u03BC\u03B1
    \u03BA\u03B1\u03B9 \u03C4\u03BF \u03C1\u03AE\u03BC\u03B1. \u0397 \u03B4\u03B9\u03B1\u03AF\u03C1\u03B5\u03C3\u03B7
    \u03C4\u03C9\u03BD \u03C0\u03C1\u03BF\u03C4\u03AC\u03C3\u03B5\u03C9\u03BD \u03C3\u03B5
    \u03C3\u03C5\u03C3\u03C4\u03B1\u03C4\u03B9\u03BA\u03AC \u03B8\u03B1 \u03B2\u03B5\u03BB\u03C4\u03B9\u03CE\u03C3\u03B5\u03B9
    \u03C4\u03B7 \u03BC\u03AC\u03B8\u03B7\u03C3\u03AE \u03C3\u03B1\u03C2 - \u03B4\u03BF\u03BA\u03B9\u03BC\u03AC\u03C3\u03C4\u03B5
    \u03C4\u03BF! \u03A4\u03BF \u03AF\u03B4\u03B9\u03BF \u03B9\u03C3\u03C7\u03CD\u03B5\u03B9
    \u03BA\u03B1\u03B9 \u03B3\u03B9\u03B1 \u03C4\u03B7\u03BD \u03B5\u03BA\u03BC\u03AC\u03B8\u03B7\u03C3\u03B7
    \u03C4\u03BF\u03C5 \u03C0\u03CE\u03C2 \u03BD\u03B1 \u03C7\u03C4\u03AF\u03B6\u03B5\u03C4\u03B5
    \u03B4\u03BF\u03BC\u03AD\u03C2\xBB"
  - "\u0391\u03BD \u03C0\u03C1\u03BF\u03C3\u03AD\u03B8\u03B5\u03C4\u03B5 \u03B4\u03AD\u03BA\u03B1
    \u03B4\u03B5\u03BA\u03AC\u03B4\u03B5\u03C2, \u03B8\u03B1 \u03AE\u03C4\u03B1\u03BD
    \u03B5\u03CD\u03BA\u03BF\u03BB\u03BF. \u03A4\u03BF \u03AC\u03B8\u03C1\u03BF\u03B9\u03C3\u03BC\u03B1
    \u03B8\u03B1 \u03AE\u03C4\u03B1\u03BD 100. \u0391\u03C5\u03C4\u03CC \u03C6\u03B1\u03AF\u03BD\u03B5\u03C4\u03B1\u03B9
    \u03B5\u03BD\u03B4\u03B9\u03B1\u03C6\u03AD\u03C1\u03BF\u03BD, \u03BF\u03C0\u03CC\u03C4\u03B5
    \u03C0\u03C1\u03BF\u03C3\u03C0\u03B1\u03B8\u03AE\u03C3\u03C4\u03B5 \u03BD\u03B1
    \u03BE\u03B5\u03BA\u03B9\u03BD\u03AE\u03C3\u03B5\u03C4\u03B5 \u03C4\u03B7\u03BD
    \u03C0\u03C1\u03CC\u03C3\u03B8\u03B5\u03C3\u03B7 \u03BC\u03B5 \u03C4\u03BF\u03BD
    \u03C4\u03B5\u03BB\u03B5\u03C5\u03C4\u03B1\u03AF\u03BF \u03B1\u03C1\u03B9\u03B8\u03BC\u03CC,
    \u03B4\u03B7\u03BB\u03B1\u03B4\u03AE 10. \u03A0\u03C1\u03BF\u03C3\u03B8\u03AD\u03C3\u03C4\u03B5
    \u03C3\u03B5 \u03B1\u03C5\u03C4\u03CC \u03C4\u03BF \u03AC\u03B8\u03C1\u03BF\u03B9\u03C3\u03BC\u03B1
    \u03C4\u03BF\u03C5 \u03C0\u03C1\u03CE\u03C4\u03BF\u03C5 \u03BA\u03B1\u03B9 \u03C4\u03BF\u03C5
    \u03B1\u03C0\u03CC \u03B4\u03B5\u03CD\u03C4\u03B5\u03C1\u03BF \u03C0\u03C1\u03BF\u03C2
    \u03C4\u03BF \u03C4\u03B5\u03BB\u03B5\u03C5\u03C4\u03B1\u03AF\u03BF, \u03B4\u03B7\u03BB\u03B1\u03B4\u03AE
    1 + 9 \u03BA\u03B1\u03B9 \u03C0\u03C1\u03BF\u03C7\u03C9\u03C1\u03AE\u03C3\u03C4\u03B5
    \u03C9\u03C2 \u03B5\u03BE\u03AE\u03C2. \u0397 \u03C0\u03C1\u03BF\u03C3\u03B8\u03AE\u03BA\u03B7
    \u03B8\u03B1 \u03BC\u03BF\u03B9\u03AC\u03B6\u03B5\u03B9 \u03BC\u03B5 \u03B1\u03C5\u03C4\u03CC:
    10 + 1 + 9 + 2 + 8 + 3 + 7 + 4 + 6. \u0388\u03C7\u03B5\u03C4\u03B5 \u03C0\u03AD\u03BD\u03C4\u03B5
    \u03C3\u03B5\u03B9\u03C1\u03AD\u03C2 \u03BA\u03B1\u03B9 \u03C3\u03B5 \u03BA\u03B1\u03B8\u03B5\u03BC\u03AF\u03B1
    \u03C4\u03BF \u03AC\u03B8\u03C1\u03BF\u03B9\u03C3\u03BC\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9
    10. \u0388\u03C4\u03C3\u03B9 \u03AD\u03C7\u03B5\u03C4\u03B5 \u03B1\u03C0\u03BF\u03C3\u03C5\u03BD\u03B8\u03AD\u03C3\u03B5\u03B9
    \u03BC\u03B9\u03B1 \u03C3\u03CD\u03BD\u03B8\u03B5\u03C4\u03B7 \u03B1\u03C1\u03B9\u03B8\u03BC\u03B7\u03C4\u03B9\u03BA\u03AE
    \u03C0\u03C1\u03AC\u03BE\u03B7 \u03C3\u03B5 \u03BC\u03B9\u03B1 \u03C3\u03B5\u03B9\u03C1\u03AC
    \u03B1\u03C0\u03CC \u03B1\u03C0\u03BB\u03AC \u03C3\u03C4\u03BF\u03B9\u03C7\u03B5\u03AF\u03B1.
    \u03A0\u03AD\u03BD\u03C4\u03B5 \u03B4\u03B5\u03BA\u03AC\u03B4\u03B5\u03C2, \u03B5\u03AF\u03BD\u03B1\u03B9
    50, \u03BA\u03B1\u03B9 50, \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C4\u03BF \u03BC\u03B9\u03C3\u03CC
    \u03C4\u03BF\u03C5 100. \u0394\u03B7\u03BB\u03B1\u03B4\u03AE, 100 / 2. \u03A3\u03B1\u03C2
    \u03BC\u03AD\u03BD\u03BF\u03C5\u03BD \u03AC\u03BB\u03BB\u03B1 5 \u03BD\u03B1
    \u03C0\u03C1\u03BF\u03C3\u03B8\u03AD\u03C3\u03B5\u03C4\u03B5. \u038C\u03BB\u03B1
    \u03BC\u03B1\u03B6\u03AF, \u03B5\u03AF\u03BD\u03B1\u03B9 100 / 2 + 5, \u03AE
    55. \u039C\u03C0\u03BF\u03C1\u03B5\u03AF\u03C4\u03B5 \u03BD\u03B1 \u03B4\u03B5\u03AF\u03C4\u03B5
    \u03C4\u03BF \u03BC\u03BF\u03C4\u03AF\u03B2\u03BF \u03C4\u03CE\u03C1\u03B1 \u03C0\u03BF\u03C5
    \u03B8\u03B1 \u03B1\u03C0\u03BB\u03BF\u03C0\u03BF\u03B9\u03AE\u03C3\u03B5\u03B9
    \u03C4\u03BF\u03C5\u03C2 \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03BC\u03BF\u03CD\u03C2;
    \u03A4\u03CE\u03C1\u03B1 \u03B5\u03BB\u03AD\u03B3\u03BE\u03C4\u03B5 \u03B1\u03BD
    \u03C4\u03BF 55 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03AD\u03BD\u03B1 \u03BA\u03B1\u03BB\u03CC
    \u03B1\u03C0\u03BF\u03C4\u03AD\u03BB\u03B5\u03C3\u03BC\u03B1 \u03C0\u03C1\u03BF\u03C3\u03B8\u03AD\u03C4\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03CC\u03BB\u03BF\u03C5\u03C2 \u03C4\u03BF\u03C5\u03C2 \u03B1\u03C1\u03B9\u03B8\u03BC\u03BF\u03CD\u03C2
    \u03B1\u03C0\u03CC \u03C4\u03BF 1 \u03AD\u03C9\u03C2 \u03C4\u03BF 10 \u03AD\u03BD\u03B1\u03BD
    \u03C0\u03C1\u03BF\u03C2 \u03AD\u03BD\u03B1\u03BD."
  - "\u0391\u03BD \u03B1\u03B8\u03C1\u03BF\u03AF\u03C3\u03B5\u03C4\u03B5 \u03C4\u03BF\u03BD
    \u03C0\u03C1\u03CE\u03C4\u03BF \u03BA\u03B1\u03B9 \u03C4\u03BF\u03BD \u03C4\u03B5\u03BB\u03B5\u03C5\u03C4\u03B1\u03AF\u03BF
    \u03B1\u03C1\u03B9\u03B8\u03BC\u03CC \u03C0\u03BF\u03C5 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9
    \u03BD\u03B1 \u03C0\u03C1\u03BF\u03C3\u03B8\u03AD\u03C3\u03B5\u03C4\u03B5, \u03C0\u03BF\u03C5
    \u03B5\u03AF\u03BD\u03B1\u03B9 1 + 20, \u03C0\u03B1\u03AF\u03C1\u03BD\u03B5\u03C4\u03B5
    21. \u0388\u03C7\u03B5\u03C4\u03B5 10 \u03C4\u03AD\u03C4\u03BF\u03B9\u03B1 \u03B6\u03B5\u03CD\u03B3\u03B7.
    \u0391\u03C5\u03C4\u03AC \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C4\u03B1 \u03B4\u03BF\u03BC\u03B9\u03BA\u03AC
    \u03C3\u03C4\u03BF\u03B9\u03C7\u03B5\u03AF\u03B1 \u03C0\u03BF\u03C5 \u03B1\u03BD\u03B1\u03BA\u03B1\u03BB\u03CD\u03C8\u03B1\u03C4\u03B5
    \u03B1\u03C0\u03BF\u03C3\u03C5\u03BD\u03B8\u03AD\u03C4\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03AD\u03BD\u03B1\u03BD \u03C0\u03BF\u03BB\u03CD\u03C0\u03BB\u03BF\u03BA\u03BF
    \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03BC\u03CC. \u0386\u03C1\u03B1
    10 x 21 = 210. \u0391\u03C5\u03C4\u03CC \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C4\u03BF
    \u03BC\u03BF\u03C4\u03AF\u03B2\u03BF \u03B3\u03B9\u03B1 \u03C4\u03AD\u03C4\u03BF\u03B9\u03BF\u03C5\u03C2
    \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03BC\u03BF\u03CD\u03C2. \u03A4\u03CE\u03C1\u03B1
    \u03B5\u03BB\u03AD\u03B3\u03BE\u03C4\u03B5 \u03B1\u03BD \u03C4\u03BF 210 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03BA\u03B1\u03BB\u03CC \u03B1\u03C0\u03BF\u03C4\u03AD\u03BB\u03B5\u03C3\u03BC\u03B1
    \u03C0\u03C1\u03BF\u03C3\u03B8\u03AD\u03C4\u03BF\u03BD\u03C4\u03B1\u03C2 \u03CC\u03BB\u03BF\u03C5\u03C2
    \u03C4\u03BF\u03C5\u03C2 \u03B1\u03C1\u03B9\u03B8\u03BC\u03BF\u03CD\u03C2 \u03B1\u03C0\u03CC
    \u03C4\u03BF 1 \u03AD\u03C9\u03C2 \u03C4\u03BF 20 \u03AD\u03BD\u03B1\u03BD \u03C0\u03C1\u03BF\u03C2
    \u03AD\u03BD\u03B1\u03BD \u03C3\u03B5 \u03AD\u03BD\u03B1 \u03BA\u03BF\u03BC\u03BC\u03AC\u03C4\u03B9
    \u03C7\u03B1\u03C1\u03C4\u03AF, \u03C3\u03B5 \u03BC\u03B9\u03B1 \u03B1\u03C1\u03B9\u03B8\u03BC\u03BF\u03BC\u03B7\u03C7\u03B1\u03BD\u03AE
    \u03AE \u03C3\u03B5 \u03AD\u03BD\u03B1\u03BD \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03AE.
    \u039C\u03C0\u03BF\u03C1\u03B5\u03AF\u03C4\u03B5 \u03B5\u03C0\u03AF\u03C3\u03B7\u03C2
    \u03BD\u03B1 \u03B4\u03BF\u03BA\u03B9\u03BC\u03AC\u03C3\u03B5\u03C4\u03B5 \u03C3\u03C4\u03B7
    \u03BC\u03BD\u03AE\u03BC\u03B7 :)"
  - "\u0393\u03B9\u03B1 \u03B1\u03C1\u03C7\u03AE, \u03AD\u03BD\u03B1 \u03C0\u03B1\u03C1\u03AC\u03B4\u03B5\u03B9\u03B3\u03BC\u03B1.
    \u0397 \u03C0\u03C1\u03BF\u03C3\u03B8\u03AE\u03BA\u03B7 \u03B1\u03C1\u03B9\u03B8\u03BC\u03CE\u03BD
    \u03B1\u03C0\u03CC \u03C4\u03BF 1 \u03AD\u03C9\u03C2 \u03C4\u03BF 10, \u03B4\u03B7\u03BB\u03B1\u03B4\u03AE
    \u03C3\u03C4\u03BF \u03A7 = 10, \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C3\u03C4\u03B7\u03BD
    \u03B1\u03C1\u03C7\u03AE \u03B5\u03CD\u03BA\u03BF\u03BB\u03B7 \u03C5\u03C0\u03CC\u03B8\u03B5\u03C3\u03B7.
    \u03A4\u03BF 1 + 2 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03BC\u03CC\u03BD\u03BF 3
    \u03BA\u03B1\u03B9 \u03C4\u03BF 3 + 3 \u03B5\u03AF\u03BD\u03B1\u03B9 6, \u03BA.\u03BB\u03C0.
    \u03A6\u03B1\u03BD\u03C4\u03B1\u03C3\u03C4\u03B5\u03AF\u03C4\u03B5 \u03CC\u03BC\u03C9\u03C2
    \u03BD\u03B1 \u03C0\u03C1\u03BF\u03C3\u03B8\u03AD\u03C3\u03B5\u03C4\u03B5 \u03B1\u03C0\u03CC
    \u03C4\u03BF 1 \u03C3\u03C4\u03BF 140. \u039A\u03B1\u03B8\u03CE\u03C2 \u03C0\u03C1\u03BF\u03C3\u03B8\u03AD\u03C4\u03B5\u03C4\u03B5
    \u03CC\u03BB\u03BF \u03BA\u03B1\u03B9 \u03C0\u03B5\u03C1\u03B9\u03C3\u03C3\u03CC\u03C4\u03B5\u03C1\u03BF\u03C5\u03C2
    \u03B1\u03C1\u03B9\u03B8\u03BC\u03BF\u03CD\u03C2 \u03C3\u03C4\u03BF \u03BC\u03C5\u03B1\u03BB\u03CC
    \u03C3\u03B1\u03C2, \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B5\u03CD\u03BA\u03BF\u03BB\u03BF
