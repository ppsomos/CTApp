    \u03CC\u03C0\u03C9\u03C2 \u03BD\u03B1 \u03BE\u03B5\u03BA\u03B9\u03BD\u03AE\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03B1\u03C0\u03CC \u03B4\u03B9\u03B1\u03C6\u03BF\u03C1\u03B5\u03C4\u03B9\u03BA\u03AD\u03C2
    \u03B8\u03AD\u03C3\u03B5\u03B9\u03C2 \u03C3\u03C4\u03B7 \u03C3\u03BA\u03B1\u03BA\u03B9\u03AD\u03C1\u03B1
    \u03AE \u03BD\u03B1 \u03B1\u03BB\u03BB\u03AC\u03BE\u03BF\u03C5\u03BC\u03B5 \u03C4\u03BF
    \u03BC\u03AD\u03B3\u03B5\u03B8\u03BF\u03C2 \u03C4\u03B7\u03C2 \u03C3\u03BA\u03B1\u03BA\u03B9\u03AD\u03C1\u03B1\u03C2.
    \u0398\u03B1 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03B5\u03C0\u03AF\u03C3\u03B7\u03C2
    \u03BD\u03B1 \u03B4\u03BF\u03BA\u03B9\u03BC\u03AC\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03B7 \u03BB\u03CD\u03C3\u03B7 \u03C3\u03B5 \u03B1\u03BA\u03BC\u03AD\u03C2,
    \u03CC\u03C0\u03C9\u03C2 \u03CC\u03C4\u03B1\u03BD \u03B7 \u03B1\u03C1\u03C7\u03B9\u03BA\u03AE
    \u03B8\u03AD\u03C3\u03B7 \u03B2\u03C1\u03AF\u03C3\u03BA\u03B5\u03C4\u03B1\u03B9
    \u03AE\u03B4\u03B7 \u03C3\u03B5 \u03BC\u03B9\u03B1 \u03B3\u03C9\u03BD\u03AF\u03B1
    \u03C4\u03B7\u03C2 \u03C3\u03BA\u03B1\u03BA\u03B9\u03AD\u03C1\u03B1\u03C2.  \u0391\u03C0\u03CC
    \u03B1\u03C5\u03C4\u03AE \u03C4\u03B7\u03BD \u03AC\u03C0\u03BF\u03C8\u03B7, \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03C3\u03B1\u03C6\u03AD\u03C2 \u03CC\u03C4\u03B9 \u03BF \u03B5\u03BB\u03AC\u03C7\u03B9\u03C3\u03C4\u03BF\u03C2
    \u03B1\u03C1\u03B9\u03B8\u03BC\u03CC\u03C2 \u03BA\u03B9\u03BD\u03AE\u03C3\u03B5\u03C9\u03BD
    \u03C0\u03BF\u03C5 \u03B1\u03C0\u03B1\u03B9\u03C4\u03B5\u03AF\u03C4\u03B1\u03B9
    \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03C6\u03C4\u03AC\u03C3\u03B5\u03B9 \u03BF \u03B9\u03C0\u03C0\u03CC\u03C4\u03B7\u03C2
    \u03C3\u03B5 \u03BC\u03AF\u03B1 \u03B1\u03C0\u03CC \u03C4\u03B9\u03C2 \u03B3\u03C9\u03BD\u03AF\u03B5\u03C2
    \u03C4\u03B7\u03C2 \u03C3\u03BA\u03B1\u03BA\u03B9\u03AD\u03C1\u03B1\u03C2 5x5
    \u03BE\u03B5\u03BA\u03B9\u03BD\u03CE\u03BD\u03C4\u03B1\u03C2 \u03B1\u03C0\u03CC
    \u03C4\u03BF \u03BA\u03B5\u03BD\u03C4\u03C1\u03B9\u03BA\u03CC \u03C4\u03B5\u03C4\u03C1\u03AC\u03B3\u03C9\u03BD\u03BF
    \u03B5\u03AF\u03BD\u03B1\u03B9 4. \u0391\u03C5\u03C4\u03CC \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF
    \u03BD\u03B1 \u03B1\u03C0\u03BF\u03B4\u03B5\u03B9\u03C7\u03B8\u03B5\u03AF \u03B1\u03BD\u03B1\u03BB\u03CD\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03CC\u03BB\u03B5\u03C2 \u03C4\u03B9\u03C2 \u03C0\u03B9\u03B8\u03B1\u03BD\u03AD\u03C2
    \u03BA\u03B9\u03BD\u03AE\u03C3\u03B5\u03B9\u03C2 \u03C0\u03BF\u03C5 \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF
    \u03BD\u03B1 \u03BA\u03AC\u03BD\u03B5\u03B9 \u03BF \u03B9\u03C0\u03C0\u03CC\u03C4\u03B7\u03C2
    \u03BA\u03B1\u03B9 \u03B2\u03C1\u03AF\u03C3\u03BA\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03C4\u03BF \u03C3\u03C5\u03BD\u03C4\u03BF\u03BC\u03CC\u03C4\u03B5\u03C1\u03BF
    \u03BC\u03BF\u03BD\u03BF\u03C0\u03AC\u03C4\u03B9 \u03C3\u03B5 \u03BC\u03B9\u03B1
    \u03B3\u03C9\u03BD\u03AF\u03B1\xBB."
  - "\u0391\u03C0\u03CC \u03C4\u03B7\u03BD \u03AC\u03C0\u03BF\u03C8\u03B7 \u03C4\u03B7\u03C2
    \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE\u03C2
    \u03C3\u03BA\u03AD\u03C8\u03B7\u03C2, \u03B7 \u03C4\u03AE\u03C1\u03B7\u03C3\u03B7
    \u03C4\u03C9\u03BD \u03B2\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD \u03BC\u03B5 \u03C4\u03B7
    \u03C3\u03B5\u03B9\u03C1\u03AC \u03C0\u03BF\u03C5 \u03B4\u03AF\u03BD\u03BF\u03BD\u03C4\u03B1\u03B9
    \u03BA\u03B1\u03C4\u03AC \u03C4\u03B7\u03BD \u03C0\u03C1\u03BF\u03B5\u03C4\u03BF\u03B9\u03BC\u03B1\u03C3\u03AF\u03B1
    \u03BC\u03B9\u03B1\u03C2 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE\u03C2 \u03BA\u03AD\u03B9\u03BA
    \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B1\u03C0\u03B1\u03C1\u03B1\u03AF\u03C4\u03B7\u03C4\u03B7
    \u03B3\u03B9\u03B1 \u03C4\u03B7 \u03B4\u03B9\u03B1\u03C3\u03C6\u03AC\u03BB\u03B9\u03C3\u03B7
    \u03B5\u03BD\u03CC\u03C2 \u03BA\u03B1\u03BB\u03BF\u03CD \u03B1\u03C0\u03BF\u03C4\u03B5\u03BB\u03AD\u03C3\u03BC\u03B1\u03C4\u03BF\u03C2.
    \u039D\u03B1 \u03B3\u03B9\u03B1\u03C4\u03AF:  1. \u0391\u03C0\u03BF\u03C3\u03CD\u03BD\u03B8\u03B5\u03C3\u03B7:
    \u03A4\u03BF \u03C0\u03C1\u03CE\u03C4\u03BF \u03B2\u03AE\u03BC\u03B1 \u03B3\u03B9\u03B1
    \u03C4\u03B7\u03BD \u03C0\u03C1\u03BF\u03B5\u03C4\u03BF\u03B9\u03BC\u03B1\u03C3\u03AF\u03B1
    \u03BC\u03B9\u03B1\u03C2 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE\u03C2 \u03B3\u03B9\u03B1
    \u03BA\u03AD\u03B9\u03BA \u03B5\u03AF\u03BD\u03B1\u03B9 \u03BD\u03B1 \u03C7\u03C9\u03C1\u03AF\u03C3\u03B5\u03C4\u03B5
    \u03C4\u03B7\u03BD \u03B5\u03C1\u03B3\u03B1\u03C3\u03AF\u03B1 \u03C3\u03B5 \u03BC\u03B9\u03BA\u03C1\u03CC\u03C4\u03B5\u03C1\u03B5\u03C2
    \u03B4\u03B5\u03C5\u03C4\u03B5\u03C1\u03B5\u03CD\u03BF\u03C5\u03C3\u03B5\u03C2
    \u03B5\u03C1\u03B3\u03B1\u03C3\u03AF\u03B5\u03C2. \u0391\u03C5\u03C4\u03CC \u03C0\u03B5\u03C1\u03B9\u03BB\u03B1\u03BC\u03B2\u03AC\u03BD\u03B5\u03B9
    \u03C4\u03B7\u03BD \u03B1\u03C0\u03B1\u03C1\u03AF\u03B8\u03BC\u03B7\u03C3\u03B7
    \u03CC\u03BB\u03C9\u03BD \u03C4\u03C9\u03BD \u03C3\u03C5\u03C3\u03C4\u03B1\u03C4\u03B9\u03BA\u03CE\u03BD,
    \u03C4\u03B7\u03BD \u03C0\u03B5\u03C1\u03B9\u03B3\u03C1\u03B1\u03C6\u03AE \u03C4\u03BF\u03C5
    \u03C4\u03C1\u03CC\u03C0\u03BF\u03C5 \u03C3\u03C5\u03BD\u03B4\u03C5\u03B1\u03C3\u03BC\u03BF\u03CD
    \u03C4\u03BF\u03C5\u03C2 \u03B3\u03B9\u03B1 \u03BA\u03AC\u03B8\u03B5 \u03C3\u03C5\u03C3\u03C4\u03B1\u03C4\u03B9\u03BA\u03CC,
    \u03C4\u03B7\u03BD \u03B5\u03BE\u03AE\u03B3\u03B7\u03C3\u03B7 \u03C0\u03B9\u03B8\u03B1\u03BD\u03CE\u03BD
    \u03C4\u03C1\u03BF\u03C0\u03BF\u03C0\u03BF\u03B9\u03AE\u03C3\u03B5\u03C9\u03BD
    \u03C3\u03C4\u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE \u03BA\u03B1\u03B9
    \u03C4\u03B7\u03BD \u03C0\u03B5\u03C1\u03B9\u03B3\u03C1\u03B1\u03C6\u03AE \u03CC\u03BB\u03C9\u03BD
    \u03C4\u03C9\u03BD \u03B2\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD \u03C0\u03BF\u03C5
    \u03B1\u03C0\u03B1\u03B9\u03C4\u03BF\u03CD\u03BD\u03C4\u03B1\u03B9 \u03B3\u03B9\u03B1
    \u03C4\u03B7\u03BD \u03C0\u03B1\u03C1\u03B1\u03C3\u03BA\u03B5\u03C5\u03AE \u03C4\u03BF\u03C5
    \u03BA\u03AD\u03B9\u03BA. \u0391\u03C5\u03C4\u03AE \u03B7 \u03B1\u03C0\u03BF\u03C3\u03CD\u03BD\u03B8\u03B5\u03C3\u03B7
    \u03BC\u03B1\u03C2 \u03B5\u03C0\u03B9\u03C4\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1
    \u03BA\u03B1\u03C4\u03B1\u03BD\u03BF\u03AE\u03C3\u03BF\u03C5\u03BC\u03B5 \u03BA\u03B1\u03BB\u03CD\u03C4\u03B5\u03C1\u03B1
    \u03C4\u03BF \u03C0\u03C1\u03CC\u03B2\u03BB\u03B7\u03BC\u03B1 \u03BA\u03B1\u03B9
    \u03BD\u03B1 \u03B5\u03C3\u03C4\u03B9\u03AC\u03C3\u03BF\u03C5\u03BC\u03B5 \u03C3\u03B5
    \u03BA\u03AC\u03B8\u03B5 \u03BC\u03B5\u03BC\u03BF\u03BD\u03C9\u03BC\u03AD\u03BD\u03B7
    \u03B5\u03C1\u03B3\u03B1\u03C3\u03AF\u03B1.  2. \u0391\u03BD\u03B1\u03B3\u03BD\u03CE\u03C1\u03B9\u03C3\u03B7
    \u03C0\u03C1\u03BF\u03C4\u03CD\u03C0\u03C9\u03BD: \u0391\u03C6\u03BF\u03CD \u03B1\u03C0\u03BF\u03C3\u03C5\u03BD\u03B8\u03AD\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03B7\u03BD \u03B5\u03C1\u03B3\u03B1\u03C3\u03AF\u03B1, \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5
    \u03BD\u03B1 \u03B1\u03BD\u03B1\u03B3\u03BD\u03C9\u03C1\u03AF\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03BC\u03BF\u03C4\u03AF\u03B2\u03B1 \u03C3\u03C4\u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE.
    \u0393\u03B9\u03B1 \u03C0\u03B1\u03C1\u03AC\u03B4\u03B5\u03B9\u03B3\u03BC\u03B1,
    \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF \u03BD\u03B1 \u03C0\u03B1\u03C1\u03B1\u03C4\u03B7\u03C1\u03AE\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03CC\u03C4\u03B9 \u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE \u03BE\u03B5\u03BA\u03B9\u03BD\u03AC
    \u03BC\u03B5 \u03C4\u03B7\u03BD \u03B1\u03C0\u03B1\u03C1\u03AF\u03B8\u03BC\u03B7\u03C3\u03B7
    \u03C4\u03C9\u03BD \u03C3\u03C5\u03C3\u03C4\u03B1\u03C4\u03B9\u03BA\u03CE\u03BD,
    \u03BC\u03B5\u03C4\u03AC \u03C0\u03C1\u03BF\u03C7\u03C9\u03C1\u03AC\u03BC\u03B5
    \u03C3\u03C4\u03B7\u03BD \u03C0\u03B5\u03C1\u03B9\u03B3\u03C1\u03B1\u03C6\u03AE
    \u03C4\u03BF\u03C5 \u03C4\u03C1\u03CC\u03C0\u03BF\u03C5 \u03C3\u03C5\u03BD\u03B4\u03C5\u03B1\u03C3\u03BC\u03BF\u03CD
    \u03C4\u03BF\u03C5\u03C2, \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03BF\u03CD\u03BC\u03B5\u03BD\u03B5\u03C2
    \u03B1\u03C0\u03CC \u03C0\u03B9\u03B8\u03B1\u03BD\u03AD\u03C2 \u03C4\u03C1\u03BF\u03C0\u03BF\u03C0\u03BF\u03B9\u03AE\u03C3\u03B5\u03B9\u03C2
    \u03C3\u03C4\u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE \u03BA\u03B1\u03B9,
    \u03C4\u03AD\u03BB\u03BF\u03C2, \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1
    \u03C0\u03BF\u03C5 \u03B1\u03C0\u03B1\u03B9\u03C4\u03BF\u03CD\u03BD\u03C4\u03B1\u03B9
    \u03B3\u03B9\u03B1 \u03C4\u03B7\u03BD \u03C0\u03B1\u03C1\u03B1\u03C3\u03BA\u03B5\u03C5\u03AE
    \u03C4\u03BF\u03C5 \u03BA\u03AD\u03B9\u03BA. \u0397 \u03B1\u03BD\u03B1\u03B3\u03BD\u03CE\u03C1\u03B9\u03C3\u03B7
    \u03B1\u03C5\u03C4\u03CE\u03BD \u03C4\u03C9\u03BD \u03BC\u03BF\u03C4\u03AF\u03B2\u03C9\u03BD
    \u03BC\u03B1\u03C2 \u03B2\u03BF\u03B7\u03B8\u03AC \u03BD\u03B1 \u03BA\u03B1\u03C4\u03B1\u03BD\u03BF\u03AE\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03B7 \u03C1\u03BF\u03AE \u03C4\u03B7\u03C2 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE\u03C2
    \u03BA\u03B1\u03B9 \u03C4\u03B9\u03C2 \u03B5\u03BE\u03B1\u03C1\u03C4\u03AE\u03C3\u03B5\u03B9\u03C2
    \u03BC\u03B5\u03C4\u03B1\u03BE\u03CD \u03C4\u03C9\u03BD \u03B4\u03B9\u03B1\u03C6\u03BF\u03C1\u03B5\u03C4\u03B9\u03BA\u03CE\u03BD
    \u03B2\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD.  3. \u0391\u03C6\u03B1\u03AF\u03C1\u03B5\u03C3\u03B7:
    \u0391\u03C6\u03BF\u03CD \u03B5\u03BD\u03C4\u03BF\u03C0\u03AF\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03B1 \u03BC\u03BF\u03C4\u03AF\u03B2\u03B1 \u03C3\u03C4\u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE,
    \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5 \u03BD\u03B1 \u03B1\u03C6\u03B1\u03B9\u03C1\u03AD\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03B9\u03C2 \u03BB\u03B5\u03C0\u03C4\u03BF\u03BC\u03AD\u03C1\u03B5\u03B9\u03B5\u03C2
    \u03BA\u03AC\u03B8\u03B5 \u03B2\u03AE\u03BC\u03B1\u03C4\u03BF\u03C2 \u03BA\u03B1\u03B9
    \u03BD\u03B1 \u03B5\u03C3\u03C4\u03B9\u03AC\u03C3\u03BF\u03C5\u03BC\u03B5 \u03C3\u03C4\u03B9\u03C2
    \u03AD\u03BD\u03BD\u03BF\u03B9\u03B5\u03C2 \u03C5\u03C8\u03B7\u03BB\u03BF\u03CD
    \u03B5\u03C0\u03B9\u03C0\u03AD\u03B4\u03BF\u03C5. \u0391\u03C5\u03C4\u03CC \u03C3\u03B7\u03BC\u03B1\u03AF\u03BD\u03B5\u03B9
    \u03CC\u03C4\u03B9 \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5 \u03BD\u03B1
    \u03C3\u03BA\u03B5\u03C6\u03C4\u03BF\u03CD\u03BC\u03B5 \u03C4\u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE
    \u03B1\u03C0\u03CC \u03C4\u03B7\u03BD \u03AC\u03C0\u03BF\u03C8\u03B7 \u03C4\u03B7\u03C2
    \u03C3\u03C5\u03BD\u03BF\u03BB\u03B9\u03BA\u03AE\u03C2 \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03B1\u03C3\u03AF\u03B1\u03C2
    \u03B1\u03BD\u03C4\u03AF \u03BD\u03B1 \u03BA\u03BF\u03BB\u03BB\u03AC\u03BC\u03B5
    \u03C3\u03B5 \u03BC\u03B5\u03BC\u03BF\u03BD\u03C9\u03BC\u03AD\u03BD\u03B1 \u03C3\u03C5\u03C3\u03C4\u03B1\u03C4\u03B9\u03BA\u03AC
    \u03AE \u03BC\u03B5\u03C4\u03C1\u03AE\u03C3\u03B5\u03B9\u03C2.  4. \u03A3\u03C7\u03B5\u03B4\u03AF\u03B1\u03C3\u03B7
    \u03B1\u03BB\u03B3\u03BF\u03C1\u03AF\u03B8\u03BC\u03BF\u03C5: \u0388\u03C7\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03C5\u03C0\u03CC\u03C8\u03B7 \u03C4\u03B9\u03C2 \u03AD\u03BD\u03BD\u03BF\u03B9\u03B5\u03C2
    \u03C5\u03C8\u03B7\u03BB\u03BF\u03CD \u03B5\u03C0\u03B9\u03C0\u03AD\u03B4\u03BF\u03C5,
    \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5 \u03BD\u03B1 \u03C3\u03C7\u03B5\u03B4\u03B9\u03AC\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03AD\u03BD\u03B1\u03BD \u03B1\u03BB\u03B3\u03CC\u03C1\u03B9\u03B8\u03BC\u03BF
    \u03B3\u03B9\u03B1 \u03C4\u03B7\u03BD \u03C0\u03C1\u03BF\u03B5\u03C4\u03BF\u03B9\u03BC\u03B1\u03C3\u03AF\u03B1
    \u03C4\u03B7\u03C2 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE\u03C2 \u03C4\u03BF\u03C5
    \u03BA\u03AD\u03B9\u03BA. \u0391\u03C5\u03C4\u03CC\u03C2 \u03BF \u03B1\u03BB\u03B3\u03CC\u03C1\u03B9\u03B8\u03BC\u03BF\u03C2
    \u03B8\u03B1 \u03C0\u03C1\u03AD\u03C0\u03B5\u03B9 \u03BD\u03B1 \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03B5\u03AF
    \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03BC\u03B5 \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC
    \u03C0\u03BF\u03C5 \u03B4\u03AF\u03BD\u03B5\u03C4\u03B1\u03B9, \u03BE\u03B5\u03BA\u03B9\u03BD\u03CE\u03BD\u03C4\u03B1\u03C2
    \u03BC\u03B5 \u03C4\u03B7\u03BD \u03B1\u03C0\u03B1\u03C1\u03AF\u03B8\u03BC\u03B7\u03C3\u03B7
    \u03C4\u03C9\u03BD \u03C3\u03C5\u03C3\u03C4\u03B1\u03C4\u03B9\u03BA\u03CE\u03BD,
    \u03BC\u03B5\u03C4\u03AC \u03C0\u03B5\u03C1\u03B9\u03B3\u03C1\u03AC\u03C6\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03C4\u03BF\u03BD \u03C4\u03C1\u03CC\u03C0\u03BF \u03C3\u03C5\u03BD\u03B4\u03C5\u03B1\u03C3\u03BC\u03BF\u03CD
    \u03C4\u03BF\u03C5\u03C2 \u03B3\u03B9\u03B1 \u03BA\u03AC\u03B8\u03B5 \u03C3\u03C5\u03C3\u03C4\u03B1\u03C4\u03B9\u03BA\u03CC,
    \u03B5\u03C0\u03B5\u03BE\u03B7\u03B3\u03CE\u03BD\u03C4\u03B1\u03C2 \u03C4\u03B9\u03C2
    \u03C0\u03B9\u03B8\u03B1\u03BD\u03AD\u03C2 \u03C4\u03C1\u03BF\u03C0\u03BF\u03C0\u03BF\u03B9\u03AE\u03C3\u03B5\u03B9\u03C2
    \u03C3\u03C4\u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE \u03BA\u03B1\u03B9,
    \u03C4\u03AD\u03BB\u03BF\u03C2, \u03C0\u03B5\u03C1\u03B9\u03B3\u03C1\u03AC\u03C6\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03CC\u03BB\u03B1 \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03C0\u03BF\u03C5
    \u03B1\u03C0\u03B1\u03B9\u03C4\u03BF\u03CD\u03BD\u03C4\u03B1\u03B9 \u03B3\u03B9\u03B1
    \u03C4\u03B7\u03BD \u03C0\u03B1\u03C1\u03B1\u03C3\u03BA\u03B5\u03C5\u03AE \u03C4\u03BF\u03C5
    \u03BA\u03AD\u03B9\u03BA. \u0391\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03CE\u03BD\u03C4\u03B1\u03C2
    \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03BC\u03B5 \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC
    \u03B4\u03B9\u03B1\u03C3\u03C6\u03B1\u03BB\u03AF\u03B6\u03B5\u03C4\u03B5 \u03CC\u03C4\u03B9
    \u03B4\u03B5\u03BD \u03B8\u03B1 \u03C7\u03AC\u03C3\u03BF\u03C5\u03BC\u03B5 \u03BA\u03B1\u03BD\u03AD\u03BD\u03B1
    \u03C3\u03B7\u03BC\u03B1\u03BD\u03C4\u03B9\u03BA\u03CC \u03B2\u03AE\u03BC\u03B1
    \u03BA\u03B1\u03B9 \u03CC\u03C4\u03B9 \u03C4\u03BF \u03BA\u03AD\u03B9\u03BA \u03B8\u03B1
    \u03B2\u03B3\u03B5\u03B9 \u03CC\u03C0\u03C9\u03C2 \u03B1\u03BD\u03B1\u03BC\u03B5\u03BD\u03CC\u03C4\u03B1\u03BD. 
    5. \u0394\u03BF\u03BA\u03B9\u03BC\u03AE: \u03A4\u03AD\u03BB\u03BF\u03C2, \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5
    \u03BD\u03B1 \u03B4\u03BF\u03BA\u03B9\u03BC\u03AC\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03CE\u03BD\u03C4\u03B1\u03C2
    \u03C4\u03BF\u03BD \u03B1\u03BB\u03B3\u03CC\u03C1\u03B9\u03B8\u03BC\u03BF \u03C0\u03BF\u03C5
    \u03AD\u03C7\u03BF\u03C5\u03BC\u03B5 \u03C3\u03C7\u03B5\u03B4\u03B9\u03AC\u03C3\u03B5\u03B9
    \u03BA\u03B1\u03B9 \u03BA\u03AC\u03BD\u03BF\u03BD\u03C4\u03B1\u03C2 \u03C4\u03C1\u03BF\u03C0\u03BF\u03C0\u03BF\u03B9\u03AE\u03C3\u03B5\u03B9\u03C2
    \u03CC\u03C0\u03C9\u03C2 \u03B1\u03C0\u03B1\u03B9\u03C4\u03B5\u03AF\u03C4\u03B1\u03B9.
    \u0397 \u03B4\u03BF\u03BA\u03B9\u03BC\u03AE \u03BC\u03B1\u03C2 \u03B2\u03BF\u03B7\u03B8\u03AC
    \u03BD\u03B1 \u03B5\u03BD\u03C4\u03BF\u03C0\u03AF\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03C5\u03C7\u03CC\u03BD \u03C3\u03C6\u03AC\u03BB\u03BC\u03B1\u03C4\u03B1
    \u03C3\u03C4\u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE \u03BA\u03B1\u03B9
    \u03BD\u03B1 \u03B2\u03B5\u03BB\u03C4\u03B9\u03CE\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03BF\u03BD \u03B1\u03BB\u03B3\u03CC\u03C1\u03B9\u03B8\u03BC\u03BF \u03B3\u03B9\u03B1
    \u03BC\u03B5\u03BB\u03BB\u03BF\u03BD\u03C4\u03B9\u03BA\u03AE \u03C7\u03C1\u03AE\u03C3\u03B7. 
    \u0393\u03B5\u03BD\u03B9\u03BA\u03AC, \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C3\u03B7\u03BC\u03B1\u03BD\u03C4\u03B9\u03BA\u03CC
    \u03BD\u03B1 \u03B1\u03BA\u03BF\u03BB\u03BF\u03C5\u03B8\u03AE\u03C3\u03B5\u03C4\u03B5
    \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03BC\u03B5 \u03C4\u03B7 \u03C3\u03B5\u03B9\u03C1\u03AC
    \u03BA\u03B1\u03C4\u03AC \u03C4\u03B7\u03BD \u03C0\u03C1\u03BF\u03B5\u03C4\u03BF\u03B9\u03BC\u03B1\u03C3\u03AF\u03B1
    \u03BC\u03B9\u03B1\u03C2 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE\u03C2 \u03B3\u03B9\u03B1
    \u03BA\u03AD\u03B9\u03BA, \u03B4\u03B9\u03CC\u03C4\u03B9 \u03B4\u03B9\u03B1\u03C3\u03C6\u03B1\u03BB\u03AF\u03B6\u03B5\u03B9
    \u03CC\u03C4\u03B9 \u03C0\u03B5\u03C1\u03B9\u03BB\u03B1\u03BC\u03B2\u03AC\u03BD\u03BF\u03BD\u03C4\u03B1\u03B9
    \u03CC\u03BB\u03B1 \u03C4\u03B1 \u03B1\u03C0\u03B1\u03C1\u03B1\u03AF\u03C4\u03B7\u03C4\u03B1
    \u03C3\u03C5\u03C3\u03C4\u03B1\u03C4\u03B9\u03BA\u03AC \u03BA\u03B1\u03B9 \u03CC\u03C4\u03B9
    \u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE \u03B5\u03BA\u03C4\u03B5\u03BB\u03B5\u03AF\u03C4\u03B1\u03B9
    \u03C3\u03C9\u03C3\u03C4\u03AC. \u0391\u03BD\u03B1\u03BB\u03CD\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03C4\u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE \u03C3\u03B5 \u03BC\u03B9\u03BA\u03C1\u03CC\u03C4\u03B5\u03C1\u03B5\u03C2
    \u03B4\u03B5\u03C5\u03C4\u03B5\u03C1\u03B5\u03CD\u03BF\u03C5\u03C3\u03B5\u03C2
    \u03B5\u03C1\u03B3\u03B1\u03C3\u03AF\u03B5\u03C2, \u03B1\u03BD\u03B1\u03B3\u03BD\u03C9\u03C1\u03AF\u03B6\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03BC\u03BF\u03C4\u03AF\u03B2\u03B1, \u03B1\u03C6\u03B1\u03B9\u03C1\u03CE\u03BD\u03C4\u03B1\u03C2
    \u03BB\u03B5\u03C0\u03C4\u03BF\u03BC\u03AD\u03C1\u03B5\u03B9\u03B5\u03C2, \u03C3\u03C7\u03B5\u03B4\u03B9\u03AC\u03B6\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03AD\u03BD\u03B1\u03BD \u03B1\u03BB\u03B3\u03CC\u03C1\u03B9\u03B8\u03BC\u03BF
    \u03BA\u03B1\u03B9 \u03B4\u03BF\u03BA\u03B9\u03BC\u03AC\u03B6\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03C4\u03B7 \u03C3\u03C5\u03BD\u03C4\u03B1\u03B3\u03AE, \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5
    \u03BD\u03B1 \u03B4\u03B9\u03B1\u03C3\u03C6\u03B1\u03BB\u03AF\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03CC\u03C4\u03B9 \u03C4\u03BF \u03BA\u03AD\u03B9\u03BA \u03B8\u03B1 \u03B2\u03B3\u03B1\u03AF\u03BD\u03B5\u03B9
    \u03BD\u03CC\u03C3\u03C4\u03B9\u03BC\u03BF \u03BA\u03AC\u03B8\u03B5 \u03C6\u03BF\u03C1\u03AC."
  - "\u0397 \u03C3\u03C4\u03B5\u03C1\u03B5\u03BF\u03C3\u03BA\u03BF\u03C0\u03B9\u03BA\u03AE
    \u03CC\u03C1\u03B1\u03C3\u03B7 \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B7 \u03B9\u03BA\u03B1\u03BD\u03CC\u03C4\u03B7\u03C4\u03B1
    \u03C4\u03C9\u03BD \u03BC\u03B1\u03C4\u03B9\u03CE\u03BD \u03BC\u03B1\u03C2 \u03BD\u03B1
    \u03B2\u03BB\u03AD\u03C0\u03BF\u03C5\u03BD \u03C4\u03BF \u03AF\u03B4\u03B9\u03BF
    \u03B1\u03BD\u03C4\u03B9\u03BA\u03B5\u03AF\u03BC\u03B5\u03BD\u03BF \u03B1\u03C0\u03CC
    \u03B5\u03BB\u03B1\u03C6\u03C1\u03CE\u03C2 \u03B4\u03B9\u03B1\u03C6\u03BF\u03C1\u03B5\u03C4\u03B9\u03BA\u03AD\u03C2
    \u03C0\u03C1\u03BF\u03BF\u03C0\u03C4\u03B9\u03BA\u03AD\u03C2, \u03BB\u03CC\u03B3\u03C9
    \u03C4\u03C9\u03BD \u03B5\u03BB\u03B1\u03C6\u03C1\u03CE\u03C2 \u03B4\u03B9\u03B1\u03C6\u03BF\u03C1\u03B5\u03C4\u03B9\u03BA\u03CE\u03BD
    \u03B8\u03AD\u03C3\u03B5\u03C9\u03BD \u03C4\u03BF\u03C5\u03C2 \u03C3\u03C4\u03BF
    \u03C0\u03C1\u03CC\u03C3\u03C9\u03C0\u03CC \u03BC\u03B1\u03C2. \u039F \u03B5\u03B3\u03BA\u03AD\u03C6\u03B1\u03BB\u03CC\u03C2
    \u03BC\u03B1\u03C2 \u03C3\u03C5\u03BD\u03B4\u03C5\u03AC\u03B6\u03B5\u03B9 \u03C3\u03C4\u03B7
    \u03C3\u03C5\u03BD\u03AD\u03C7\u03B5\u03B9\u03B1 \u03B1\u03C5\u03C4\u03AD\u03C2
    \u03C4\u03B9\u03C2 \u03B4\u03CD\u03BF \u03B5\u03B9\u03BA\u03CC\u03BD\u03B5\u03C2
    \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03B4\u03B7\u03BC\u03B9\u03BF\u03C5\u03C1\u03B3\u03AE\u03C3\u03B5\u03B9
    \u03BC\u03B9\u03B1 \u03C4\u03C1\u03B9\u03C3\u03B4\u03B9\u03AC\u03C3\u03C4\u03B1\u03C4\u03B7
    \u03B1\u03BD\u03C4\u03AF\u03BB\u03B7\u03C8\u03B7 \u03C4\u03BF\u03C5 \u03B1\u03BD\u03C4\u03B9\u03BA\u03B5\u03B9\u03BC\u03AD\u03BD\u03BF\u03C5. 
    \u039C\u03B9\u03B1 \u03C3\u03C4\u03B5\u03C1\u03B5\u03BF\u03B3\u03C1\u03B1\u03C6\u03B9\u03BA\u03AE
    \u03B5\u03B9\u03BA\u03CC\u03BD\u03B1 \u03B5\u03BA\u03BC\u03B5\u03C4\u03B1\u03BB\u03BB\u03B5\u03CD\u03B5\u03C4\u03B1\u03B9
    \u03B1\u03C5\u03C4\u03CC \u03C4\u03BF \u03C6\u03C5\u03C3\u03B9\u03BA\u03CC \u03C6\u03B1\u03B9\u03BD\u03CC\u03BC\u03B5\u03BD\u03BF
    \u03C0\u03B1\u03C1\u03BF\u03C5\u03C3\u03B9\u03AC\u03B6\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03B4\u03CD\u03BF \u03B5\u03BB\u03B1\u03C6\u03C1\u03CE\u03C2 \u03B4\u03B9\u03B1\u03C6\u03BF\u03C1\u03B5\u03C4\u03B9\u03BA\u03AD\u03C2
    \u03B5\u03B9\u03BA\u03CC\u03BD\u03B5\u03C2 \u03B4\u03AF\u03C0\u03BB\u03B1 \u03B4\u03AF\u03C0\u03BB\u03B1.
    \u038C\u03C4\u03B1\u03BD \u03C4\u03BF\u03BD \u03B4\u03BF\u03CD\u03BC\u03B5 \u03C3\u03C9\u03C3\u03C4\u03AC,
    \u03BF \u03B5\u03B3\u03BA\u03AD\u03C6\u03B1\u03BB\u03CC\u03C2 \u03BC\u03B1\u03C2
    \u03C3\u03C5\u03BD\u03B4\u03C5\u03AC\u03B6\u03B5\u03B9 \u03B1\u03C5\u03C4\u03AD\u03C2
    \u03C4\u03B9\u03C2 \u03B4\u03CD\u03BF \u03B5\u03B9\u03BA\u03CC\u03BD\u03B5\u03C2
    \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03B4\u03B7\u03BC\u03B9\u03BF\u03C5\u03C1\u03B3\u03AE\u03C3\u03B5\u03B9
    \u03C4\u03B7\u03BD \u03C8\u03B5\u03C5\u03B4\u03B1\u03AF\u03C3\u03B8\u03B7\u03C3\u03B7
    \u03BC\u03B9\u03B1\u03C2 \u03C4\u03C1\u03B9\u03C3\u03B4\u03B9\u03AC\u03C3\u03C4\u03B1\u03C4\u03B7\u03C2
    \u03B5\u03B9\u03BA\u03CC\u03BD\u03B1\u03C2.  \u03A4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1
    \u03C0\u03BF\u03C5 \u03B1\u03BD\u03B1\u03C6\u03AD\u03C1\u03BF\u03BD\u03C4\u03B1\u03B9
    \u03C3\u03C4\u03B7\u03BD \u03B5\u03C1\u03CE\u03C4\u03B7\u03C3\u03B7 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03BC\u03B9\u03B1 \u03BC\u03AD\u03B8\u03BF\u03B4\u03BF\u03C2 \u03B5\u03BA\u03C0\u03B1\u03AF\u03B4\u03B5\u03C5\u03C3\u03B7\u03C2
    \u03B3\u03B9\u03B1 \u03C4\u03B1 \u03BC\u03AC\u03C4\u03B9\u03B1 \u03BA\u03B1\u03B9
    \u03C4\u03BF\u03BD \u03B5\u03B3\u03BA\u03AD\u03C6\u03B1\u03BB\u03CC \u03BC\u03B1\u03C2
    \u03BD\u03B1 \u03B4\u03BF\u03C5\u03BD \u03C4\u03B7\u03BD \u03C4\u03C1\u03B9\u03C3\u03B4\u03B9\u03AC\u03C3\u03C4\u03B1\u03C4\u03B7
    \u03B5\u03B9\u03BA\u03CC\u03BD\u03B1 \u03C3\u03B5 \u03C3\u03C4\u03B5\u03C1\u03B5\u03CC\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1. 
    \u0391\u03C0\u03CC \u03C4\u03B7 \u03C3\u03BA\u03BF\u03C0\u03B9\u03AC \u03C4\u03B7\u03C2
    \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE\u03C2
    \u03C3\u03BA\u03AD\u03C8\u03B7\u03C2, \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5
    \u03BD\u03B1 \u03C3\u03BA\u03B5\u03C6\u03C4\u03BF\u03CD\u03BC\u03B5 \u03B1\u03C5\u03C4\u03AC
    \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03C9\u03C2 \u03BC\u03B9\u03B1
    \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03B1\u03C3\u03AF\u03B1 \u03B5\u03BA\u03C0\u03B1\u03AF\u03B4\u03B5\u03C5\u03C3\u03B7\u03C2
    \u03C4\u03BF\u03C5 \u03C3\u03C5\u03C3\u03C4\u03AE\u03BC\u03B1\u03C4\u03BF\u03C2
    \u03BF\u03C0\u03C4\u03B9\u03BA\u03AE\u03C2 \u03BC\u03B1\u03C2 \u03B1\u03BD\u03C4\u03AF\u03BB\u03B7\u03C8\u03B7\u03C2
    \u03B3\u03B9\u03B1 \u03C4\u03B7\u03BD \u03B5\u03C0\u03B5\u03BE\u03B5\u03C1\u03B3\u03B1\u03C3\u03AF\u03B1
    \u03BC\u03B9\u03B1\u03C2 \u03C3\u03C4\u03B5\u03C1\u03B5\u03BF\u03B3\u03C1\u03B1\u03C6\u03B9\u03BA\u03AE\u03C2
    \u03B5\u03B9\u03BA\u03CC\u03BD\u03B1\u03C2. \u0391\u03C5\u03C4\u03AE \u03B7 \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03B1\u03C3\u03AF\u03B1
    \u03C0\u03B5\u03C1\u03B9\u03BB\u03B1\u03BC\u03B2\u03AC\u03BD\u03B5\u03B9 \u03BC\u03B9\u03B1
    \u03C3\u03B5\u03B9\u03C1\u03AC \u03B2\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD \u03C0\u03BF\u03C5
    \u03BC\u03B1\u03C2 \u03B5\u03C0\u03B9\u03C4\u03C1\u03AD\u03C0\u03BF\u03C5\u03BD
    \u03BD\u03B1 \u03BE\u03B5\u03C0\u03B5\u03C1\u03AC\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03B9\u03C2 \u03C6\u03C5\u03C3\u03B9\u03BA\u03AD\u03C2 \u03B3\u03BD\u03C9\u03C3\u03C4\u03B9\u03BA\u03AD\u03C2
    \u03C0\u03C1\u03BF\u03BA\u03B1\u03C4\u03B1\u03BB\u03AE\u03C8\u03B5\u03B9\u03C2
    \u03BA\u03B1\u03B9 \u03BD\u03B1 \u03C0\u03C1\u03BF\u03C3\u03B1\u03C1\u03BC\u03CC\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03BF \u03BF\u03C0\u03C4\u03B9\u03BA\u03CC \u03BC\u03B1\u03C2 \u03C3\u03CD\u03C3\u03C4\u03B7\u03BC\u03B1
    \u03B1\u03BD\u03C4\u03AF\u03BB\u03B7\u03C8\u03B7\u03C2 \u03B3\u03B9\u03B1 \u03BD\u03B1
    \u03B4\u03B7\u03BC\u03B9\u03BF\u03C5\u03C1\u03B3\u03AE\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03B7\u03BD \u03C8\u03B5\u03C5\u03B4\u03B1\u03AF\u03C3\u03B8\u03B7\u03C3\u03B7
    \u03BC\u03B9\u03B1\u03C2 \u03C4\u03C1\u03B9\u03C3\u03B4\u03B9\u03AC\u03C3\u03C4\u03B1\u03C4\u03B7\u03C2
    \u03B5\u03B9\u03BA\u03CC\u03BD\u03B1\u03C2.  \u039C\u03B9\u03B1 \u03C0\u03C1\u03BF\u03C3\u03AD\u03B3\u03B3\u03B9\u03C3\u03B7
    \u03C5\u03C0\u03BF\u03BB\u03BF\u03B3\u03B9\u03C3\u03C4\u03B9\u03BA\u03AE\u03C2
    \u03C3\u03BA\u03AD\u03C8\u03B7\u03C2 \u03C0\u03B5\u03C1\u03B9\u03BB\u03B1\u03BC\u03B2\u03AC\u03BD\u03B5\u03B9
    \u03C4\u03B7\u03BD \u03BA\u03B1\u03C4\u03B1\u03BD\u03BF\u03BC\u03AE \u03B5\u03BD\u03CC\u03C2
    \u03C0\u03C1\u03BF\u03B2\u03BB\u03AE\u03BC\u03B1\u03C4\u03BF\u03C2 \u03C3\u03B5
    \u03BC\u03B9\u03B1 \u03C3\u03B5\u03B9\u03C1\u03AC \u03B1\u03C0\u03CC \u03BC\u03B9\u03BA\u03C1\u03CC\u03C4\u03B5\u03C1\u03B1
    \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03BA\u03B1\u03B9 \u03B1\u03BB\u03B3\u03CC\u03C1\u03B9\u03B8\u03BC\u03BF\u03C5\u03C2
    \u03C0\u03BF\u03C5 \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BD \u03BD\u03B1 \u03B5\u03BA\u03C4\u03B5\u03BB\u03B5\u03C3\u03C4\u03BF\u03CD\u03BD
    \u03BC\u03B5 \u03BB\u03BF\u03B3\u03B9\u03BA\u03AE \u03C3\u03B5\u03B9\u03C1\u03AC.
    \u03A3\u03C4\u03B7\u03BD \u03C0\u03B5\u03C1\u03AF\u03C0\u03C4\u03C9\u03C3\u03B7
    \u03C0\u03C1\u03BF\u03B2\u03BF\u03BB\u03AE\u03C2 \u03B5\u03BD\u03CC\u03C2 \u03C3\u03C4\u03B5\u03C1\u03B5\u03BF\u03B3\u03C1\u03AC\u03BC\u03BC\u03B1\u03C4\u03BF\u03C2,
    \u03C4\u03B1 \u03B2\u03AE\u03BC\u03B1\u03C4\u03B1 \u03C0\u03BF\u03C5 \u03B1\u03BD\u03B1\u03C6\u03AD\u03C1\u03BF\u03BD\u03C4\u03B1\u03B9
    \u03C3\u03C4\u03B7\u03BD \u03B5\u03C1\u03CE\u03C4\u03B7\u03C3\u03B7 \u03BC\u03B1\u03C2
    \u03B2\u03BF\u03B7\u03B8\u03BF\u03CD\u03BD \u03BD\u03B1 \u03B5\u03BA\u03C0\u03B1\u03B9\u03B4\u03B5\u03CD\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03BF \u03C3\u03CD\u03C3\u03C4\u03B7\u03BC\u03B1 \u03BF\u03C0\u03C4\u03B9\u03BA\u03AE\u03C2
    \u03BC\u03B1\u03C2 \u03B1\u03BD\u03C4\u03AF\u03BB\u03B7\u03C8\u03B7\u03C2.  \u03A4\u03BF
    \u03C0\u03C1\u03CE\u03C4\u03BF \u03B2\u03AE\u03BC\u03B1, \u03B7 \u03C7\u03B1\u03BB\u03AC\u03C1\u03C9\u03C3\u03B7
    \u03C4\u03C9\u03BD \u03BC\u03B1\u03C4\u03B9\u03CE\u03BD, \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF
    \u03BD\u03B1 \u03B8\u03B5\u03C9\u03C1\u03B7\u03B8\u03B5\u03AF \u03C9\u03C2 \u03B1\u03C0\u03BF\u03C3\u03CD\u03BD\u03B8\u03B5\u03C3\u03B7,
    \u03BE\u03B5\u03C0\u03B5\u03C1\u03BD\u03CE\u03BD\u03C4\u03B1\u03C2 \u03C4\u03B7
    \u03C6\u03C5\u03C3\u03B9\u03BA\u03AE \u03C4\u03AC\u03C3\u03B7 \u03BD\u03B1 \u03B5\u03C3\u03C4\u03B9\u03AC\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C3\u03B5 \u03AD\u03BD\u03B1 \u03C3\u03B7\u03BC\u03B5\u03AF\u03BF. \u03A7\u03B1\u03BB\u03B1\u03C1\u03CE\u03BD\u03BF\u03BD\u03C4\u03B1\u03C2
    \u03C4\u03B1 \u03BC\u03AC\u03C4\u03B9\u03B1, \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5
    \u03BD\u03B1 \u03B4\u03BF\u03CD\u03BC\u03B5 \u03B5\u03B9\u03BA\u03CC\u03BD\u03B5\u03C2
    \u03C3\u03B5 \u03AD\u03BD\u03B1 \u03C3\u03C4\u03B5\u03C1\u03B5\u03CC\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1
    \u03C0\u03BF\u03C5 \u03B4\u03B5\u03BD \u03B2\u03BB\u03AD\u03C0\u03B1\u03BC\u03B5
    \u03C0\u03C1\u03B9\u03BD, \u03C7\u03C9\u03C1\u03AF\u03C2 \u03BD\u03B1 \u03B5\u03C3\u03C4\u03B9\u03AC\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C3\u03B5 \u03BA\u03B1\u03BC\u03AF\u03B1 \u03B1\u03C0\u03CC \u03B1\u03C5\u03C4\u03AD\u03C2. 
    \u03A4\u03BF \u03B4\u03B5\u03CD\u03C4\u03B5\u03C1\u03BF \u03B2\u03AE\u03BC\u03B1,
    \u03BA\u03BF\u03B9\u03C4\u03AC\u03B6\u03BF\u03BD\u03C4\u03B1\u03C2 \u03C0\u03AD\u03C1\u03B1
    \u03B1\u03C0\u03CC \u03C4\u03B7\u03BD \u03B5\u03B9\u03BA\u03CC\u03BD\u03B1, \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF
    \u03BD\u03B1 \u03B8\u03B5\u03C9\u03C1\u03B7\u03B8\u03B5\u03AF \u03C9\u03C2 \u03C5\u03C0\u03AD\u03C1\u03B2\u03B1\u03C3\u03B7
    \u03C4\u03B7\u03C2 \u03C6\u03C5\u03C3\u03B9\u03BA\u03AE\u03C2 \u03B3\u03BD\u03C9\u03C3\u03C4\u03B9\u03BA\u03AE\u03C2
    \u03C4\u03AC\u03C3\u03B7\u03C2 \u03BD\u03B1 \u03B5\u03C3\u03C4\u03B9\u03AC\u03B6\u03BF\u03C5\u03BC\u03B5
    \u03C3\u03B5 \u03B1\u03BD\u03C4\u03B9\u03BA\u03B5\u03AF\u03BC\u03B5\u03BD\u03B1
    \u03C3\u03C4\u03BF \u03BF\u03C0\u03C4\u03B9\u03BA\u03CC \u03BC\u03B1\u03C2 \u03C0\u03B5\u03B4\u03AF\u03BF.
    \u039A\u03BF\u03B9\u03C4\u03CE\u03BD\u03C4\u03B1\u03C2 \u03C0\u03AD\u03C1\u03B1
    \u03B1\u03C0\u03CC \u03C4\u03B7\u03BD \u03B5\u03B9\u03BA\u03CC\u03BD\u03B1, \u03B5\u03AF\u03BC\u03B1\u03C3\u03C4\u03B5
    \u03C3\u03B5 \u03B8\u03AD\u03C3\u03B7 \u03BD\u03B1 \u03C3\u03C5\u03BC\u03C0\u03B5\u03C1\u03B9\u03BB\u03AC\u03B2\u03BF\u03C5\u03BC\u03B5
    \u03BA\u03B1\u03B9 \u03C4\u03B9\u03C2 \u03B4\u03CD\u03BF \u03B5\u03B9\u03BA\u03CC\u03BD\u03B5\u03C2
    \u03C3\u03B5 \u03AD\u03BD\u03B1 \u03C3\u03C4\u03B5\u03C1\u03B5\u03CC\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1
    \u03C7\u03C9\u03C1\u03AF\u03C2 \u03BD\u03B1 \u03B5\u03C3\u03C4\u03B9\u03AC\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C3\u03B5 \u03BA\u03B1\u03BC\u03AF\u03B1 \u03B1\u03C0\u03CC \u03B1\u03C5\u03C4\u03AD\u03C2.
    \u038C\u03C4\u03B1\u03BD \u03BA\u03BF\u03B9\u03C4\u03AC\u03B6\u03BF\u03C5\u03BC\u03B5
    \u03C0\u03AD\u03C1\u03B1 \u03B1\u03C0\u03CC \u03C4\u03B7\u03BD \u03B5\u03B9\u03BA\u03CC\u03BD\u03B1,
    \u03C3\u03C5\u03BD\u03AE\u03B8\u03C9\u03C2 \u03B1\u03BD\u03C4\u03B9\u03BB\u03B1\u03BC\u03B2\u03B1\u03BD\u03CC\u03BC\u03B1\u03C3\u03C4\u03B5
    \u03C4\u03B7\u03BD \u03B5\u03B9\u03BA\u03CC\u03BD\u03B1 \u03C0\u03BF\u03C5 \u03BA\u03C1\u03CD\u03B2\u03B5\u03C4\u03B1\u03B9
    \u03C3\u03C4\u03BF \u03C3\u03C4\u03B5\u03C1\u03B5\u03CC\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1,
    \u03B7 \u03BF\u03C0\u03BF\u03AF\u03B1 \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF \u03BD\u03B1
    \u03B1\u03BD\u03B1\u03C6\u03AD\u03C1\u03B5\u03C4\u03B1\u03B9 \u03C9\u03C2 \u03B1\u03BD\u03B1\u03B3\u03BD\u03CE\u03C1\u03B9\u03C3\u03B7
    \u03C0\u03C1\u03BF\u03C4\u03CD\u03C0\u03C9\u03BD.  \u03A4\u03BF \u03C4\u03C1\u03AF\u03C4\u03BF
    \u03B2\u03AE\u03BC\u03B1, \u03B7 \u03B1\u03C0\u03BF\u03BC\u03AC\u03BA\u03C1\u03C5\u03BD\u03C3\u03B7
    \u03C4\u03BF\u03C5 \u03C3\u03C4\u03B5\u03C1\u03B5\u03BF\u03B3\u03C1\u03AC\u03BC\u03BC\u03B1\u03C4\u03BF\u03C2/\u03BA\u03B5\u03C6\u03B1\u03BB\u03B9\u03BF\u03CD,
    \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF \u03BD\u03B1 \u03B8\u03B5\u03C9\u03C1\u03B7\u03B8\u03B5\u03AF
    \u03C9\u03C2 \u03C0\u03C1\u03BF\u03C3\u03B1\u03C1\u03BC\u03BF\u03B3\u03AE \u03C4\u03BF\u03C5
    \u03C3\u03C5\u03C3\u03C4\u03AE\u03BC\u03B1\u03C4\u03BF\u03C2 \u03BF\u03C0\u03C4\u03B9\u03BA\u03AE\u03C2
    \u03BC\u03B1\u03C2 \u03B1\u03BD\u03C4\u03AF\u03BB\u03B7\u03C8\u03B7\u03C2 \u03C3\u03C4\u03B9\u03C2
    \u03B4\u03CD\u03BF \u03B5\u03B9\u03BA\u03CC\u03BD\u03B5\u03C2 \u03C3\u03C4\u03BF
    \u03C3\u03C4\u03B5\u03C1\u03B5\u03CC\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1. \u039C\u03B5\u03C4\u03B1\u03BA\u03B9\u03BD\u03CE\u03BD\u03C4\u03B1\u03C2
    \u03C4\u03BF \u03C3\u03C4\u03B5\u03C1\u03B5\u03CC\u03B3\u03C1\u03B1\u03BC\u03BC\u03B1/\u03BA\u03B5\u03C6\u03AC\u03BB\u03B9
    \u03BC\u03B1\u03BA\u03C1\u03B9\u03AC, \u03B5\u03AF\u03BC\u03B1\u03C3\u03C4\u03B5
    \u03C3\u03B5 \u03B8\u03AD\u03C3\u03B7 \u03BD\u03B1 \u03C0\u03C1\u03BF\u03C3\u03B1\u03C1\u03BC\u03CC\u03C3\u03BF\u03C5\u03BC\u03B5
    \u03C4\u03B7\u03BD \u03BF\u03C0\u03C4\u03B9\u03BA\u03AE \u03BC\u03B1\u03C2 \u03C0\u03C1\u03BF\u03BF\u03C0\u03C4\u03B9\u03BA\u03AE
    \u03AD\u03C4\u03C3\u03B9 \u03CE\u03C3\u03C4\u03B5 \u03BF\u03B9 \u03B4\u03CD\u03BF
    \u03B5\u03B9\u03BA\u03CC\u03BD\u03B5\u03C2 \u03BD\u03B1 \u03B5\u03AF\u03BD\u03B1\u03B9
    \u03C3\u03C9\u03C3\u03C4\u03AC \u03B5\u03C5\u03B8\u03C5\u03B3\u03C1\u03B1\u03BC\u03BC\u03B9\u03C3\u03BC\u03AD\u03BD\u03B5\u03C2.
    \u0392\u03BB\u03AD\u03C0\u03BF\u03BD\u03C4\u03B1\u03C2 \u03C4\u03B7 \u03C0\u03C1\u03CC\u03C3\u03C6\u03B1\u03C4\u03B1
